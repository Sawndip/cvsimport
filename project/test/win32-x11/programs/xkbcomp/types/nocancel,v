@// $Xorg: nocancel,v 1.3 2000/08/17 19:54:48 cpqbld Exp $
// 
partial default xkb_types "nocancel" {
    // alternate definition for the
    // required "ALPHABETIC" type.
    // This definition does *not* 
    // implement shift-cancels-caps
    type "ALPHABETIC" {
	modifiers = Shift;
	map[Shift] = Level2;
	level_name[Level1] = "Base";
	level_name[Level2] = "Caps";
    };
};
@



PROGRAM = editres.exe

all:: editres.exe

editres.exe: $(OBJS:.o=.obj) $(DEPLIBS)
	if exist $@@ $(RM) $@@
	$(CCLINK) -Fe$@@ $(LDOPTIONS) $(OBJS:.o=.obj) $(LOCAL_LIBRARIES) $(LDLIBS) $(EXTRA_LOAD_FLAGS)

XVAR0000 = $(DESTDIR)$(BINDIR)

install:: editres.exe
	-$(MKDIRHIER) $(XVAR0000:/=\)
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS) editres.exe $(XVAR0000:/=\)\\editres.exe

XVAR0001 = $(DESTDIR)$(MANDIR)

install.man:: editres.man
	-$(MKDIRHIER) $(XVAR0001:/=\)
	$(INSTALL) $(INSTALLFLAGS) $(INSTMANFLAGS) editres.man $(XVAR0001:/=\)\\editres.$(MANSUFFIX)

depend:: $(DEPEND).exe


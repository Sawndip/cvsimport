 <<tmp.bat
 cd $(DEPENDSRC:/=\)
 $(MAKE)
<<

depend::
	$(DEPEND:/=\) @@<<
$(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)
<<


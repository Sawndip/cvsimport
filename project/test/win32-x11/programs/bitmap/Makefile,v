DEFINES = $(IEEE_DEFS) $(SIGNAL_DEFINES)
DEPLIBS = $(DEPXAWLIB) $(DEPXMULIB) $(DEPXTOOLLIB) $(DEPXLIB)
LOCAL_LIBRARIES = $(XAWLIB) $(XMULIB) $(XTOOLLIB) $(XLIB)
SRCS1 = BitEdit.c CutPaste.c Graphics.c ReqMach.c Bitmap.c \
                  Dialog.c Handlers.c
OBJS1 = BitEdit.o CutPaste.o Graphics.o ReqMach.o Bitmap.o \
                  Dialog.o Handlers.o
SRCS2 = bmtoa.c
OBJS2 = bmtoa.o
SRCS3 = atobm.c
OBJS3 = atobm.o
PROGRAMS = bitmap bmtoa atobm

OBJS = $(OBJS1) $(OBJS2) $(OBJS3)
SRCS = $(SRCS1) $(SRCS2) $(SRCS3)
PROGRAM1 = bitmap.exe

all:: $(PROGRAM1) $(PROGRAM2) $(PROGRAM3)

bitmap.exe: $(OBJS1:.o=.obj) $(DEPLIBS1)
	if exist $@@ $(RM) $@@
	$(CCLINK) -Fe$@@ $(LDOPTIONS) $(OBJS1:.o=.obj) $(LOCAL_LIBRARIES) $(LDLIBS) $(EXTRA_LOAD_FLAGS)

XVAR0000 = $(DESTDIR)$(BINDIR)

install:: bitmap.exe
	-$(MKDIRHIER) $(XVAR0000:/=\)
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS) bitmap.exe $(XVAR0000:/=\)\\bitmap.exe

XVAR0001 = $(DESTDIR)$(MANDIR)

install.man:: bitmap.man
	-$(MKDIRHIER) $(XVAR0001:/=\)
	$(INSTALL) $(INSTALLFLAGS) $(INSTMANFLAGS) bitmap.man $(XVAR0001:/=\)\\bitmap.$(MANSUFFIX)

depend:: $(DEPEND).exe

$(DEPEND).exe:
 <<tmp.bat
 cd $(DEPENDSRC:/=\)
 $(MAKE)
<<

depend::
	$(DEPEND:/=\) @@<<
$(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)
<<

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

clean::
	for %%i in ($(PROGRAMS)) do $(RM) %%i.exe

XVAR0002 = $(OBJS2)

bmtoa.exe: $(XVAR0002:.o=.obj)
	if exist $@@ $(RM) $@@
	$(CCLINK) -Fe$@@ $(LDOPTIONS) $(XVAR0002:.o=.obj) $(XMULIB) $(LDLIBS) $(EXTRA_LOAD_FLAGS)

clean::
	if exist bmtoa.exe $(RM) bmtoa.exe

XVAR0003 = $(OBJS3)

atobm.exe: $(XVAR0003:.o=.obj)
	if exist $@@ $(RM) $@@
	$(CCLINK) -Fe$@@ $(LDOPTIONS) $(XVAR0003:.o=.obj)  $(LDLIBS) $(EXTRA_LOAD_FLAGS)

clean::
	if exist atobm.exe $(RM) atobm.exe

XVAR0004 = $(BUILDINCDIR)/X11/bitmaps
XVAR0005 = $(BUILDINCTOP)/../../$(CURRENT_DIR)

includes:: $(BUTTONPICTURES)
	-$(MKDIRHIER) $(XVAR0004:/=\)
	@@$(MAKE) -A $(MFLAGS) -f <<
.SUFFIXES:

all: $(BUTTONPICTURES)
	cd $(XVAR0004:/=\)
	!if exist $$** $(RM) $$**
	!$(LN) $(XVAR0005:/=\)\$$** .

Avoid.Error $(BUTTONPICTURES):
<<


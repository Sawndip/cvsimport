install.man::
	@@(TMP=/tmp/tmp.$$$$; \
	if exist $${TMP} $(RM) $${TMP}; \
	echo .so `basename $(MANDIR)`/bitmap.$(MANSUFFIX) > $${TMP}; \
	for i in bmtoa atobm; do (set -x; \
	$(INSTALL) $(INSTALLFLAGS) $(INSTMANFLAGS) $${TMP} $(DESTDIR)$(MANDIR)/$$i.$(MANSUFFIX)); \
	done; \
	if exist $${TMP} $(RM) $${TMP})


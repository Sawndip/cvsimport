 @@cd $@@
 @@echo making all in $(CURRENT_DIR)/$@@...
 $(MAKE) $(MFLAGS) CDEBUGFLAGS="$(CDEBUGFLAGS)" LDSTRIPFLAGS="$(LDSTRIPFLAGS)" all

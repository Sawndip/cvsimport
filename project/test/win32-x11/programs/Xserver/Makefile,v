XWINLIBS = $(DIX) $(OS) $(FONT) $(XPDDX) GL/dixGL.lib GL/opengl/GLcore.lib $(XWIN) $(CFB) $(MFB) $(XPFBLIBS) $(MI) $(EXTENSIONS) $(MI) $(RENDER)
XWINSYSLIBS = $(FONTLIBS) $(LDPRELIBS) $(XLIB) $(SYSLIBS)

XVAR0004 = $(XWINOBJS)

all:: XWin.exe
XWin.exe: $(XWINDIRS) $(XVAR0004:.o=.obj) $(XWINLIBS)
	-@@if exist $@@ $(MV) $@@ $@@.bak
	$(CCLINK) -Fe$@@ $(LDOPTIONS) $(XVAR0004:.o=.obj) $(XWINLIBS) $(LDLIBS) $(XWINSYSLIBS) $(EXTRA_LOAD_FLAGS)

loadXWin:
	-@@if exist XWin.exe $(MV) XWin.exe XWin.exe.bak
	$(CCLINK) -FeXWin.exe $(LDOPTIONS) $(XVAR0004:.o=.obj) $(XWINLIBS) $(LDLIBS) $(XWINSYSLIBS) $(EXTRA_LOAD_FLAGS)

loadX:: loadXWin

XVAR0005 = $(DESTDIR)$(BINDIR)

install:: XWin.exe
	-$(MKDIRHIER) $(XVAR0005:/=\)
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS) $(_NOOP_) XWin.exe $(XVAR0005:/=\)\\XWin.exe

clean::
	if exist XWin.exe $(RM) XWin.exe

CFBDIRS = $(CFB8DIR) $(CFB16DIR) $(CFB24DIR) $(CFB32DIR)
DDXDIRS = $(DDXDIR1) $(DDXDIR2) $(DDXDIR3) $(XVFBDDXDIR) $(XNESTDDXDIR)
SUBDIRS = $(STDDIRS) $(MFBDIR) $(CFBDIRS) $(LMFCFBDIR) $(DDXDIRS) $(GLXLIBDIR)

install::
	-(cd $(DESTDIR)$(BINDIR); $(RM) X; $(LN) XWin X)

XVAR0006 = $(SUBDIRS)
XVAR0007 = $$

depend::
	@@$(MAKE) -A $(MFLAGS) -f <<
.SUFFIXES:

all: $(XVAR0006:/=\)

Avoid.Error $(XVAR0006:/=\):
 cd $(XVAR0007)@@
 @@echo "depending" in $(CURRENT_DIR)/$(XVAR0007)@@...
 $(MAKE) $(MFLAGS)  depend
 cd ..
<<


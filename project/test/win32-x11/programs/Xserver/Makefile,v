#
# This turns off the default rule for compiling .c files because
# this makefile does not really know how to build it.  This is really
# just a hack because of the Sun version of make and nfs.
#
.c.o:

#
# Server with Xlib-based ddx
#

XNESTDDXDIR = hw

XNESTDIRS = $(STDDIRS) $(XNESTDDXDIR) $(DEPDIRS)

XNESTOBJS = dix/main.o

XNEST = hw/xnest/xnest.lib
XNESTLIBS = $(DIX) $(OS) $(FONT) $(XPDDX) GL/dixGL.lib GL/opengl/GLcore.lib $(XNEST) $(EXTENSIONS) $(XPFBLIBS) $(MI) $(EXTENSIONS) $(XNEST)
XNESTSYSLIBS = $(LDPRELIBS) $(XLIB) $(SYSLIBS)

XVAR0000 = $(XNESTOBJS)

all:: Xnest.exe
Xnest.exe: $(XNESTDIRS) $(XVAR0000:.o=.obj) $(XNESTLIBS)
	-@@if exist $@@ $(MV) $@@ $@@.bak
	$(CCLINK) -Fe$@@ $(LDOPTIONS) $(XVAR0000:.o=.obj) $(XNESTLIBS) $(LDLIBS) $(XNESTSYSLIBS) $(EXTRA_LOAD_FLAGS)

loadXnest:
	-@@if exist Xnest.exe $(MV) Xnest.exe Xnest.exe.bak
	$(CCLINK) -FeXnest.exe $(LDOPTIONS) $(XVAR0000:.o=.obj) $(XNESTLIBS) $(LDLIBS) $(XNESTSYSLIBS) $(EXTRA_LOAD_FLAGS)

loadX:: loadXnest

XVAR0001 = $(DESTDIR)$(BINDIR)

install:: Xnest.exe
	-$(MKDIRHIER) $(XVAR0001:/=\)
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS) $(_NOOP_) Xnest.exe $(XVAR0001:/=\)\\Xnest.exe

clean::
	if exist Xnest.exe $(RM) Xnest.exe


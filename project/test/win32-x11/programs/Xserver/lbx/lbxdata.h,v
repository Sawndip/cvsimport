extern int  lbx_font_private;

typedef struct _fonttaginfo {
    XID		tid;
    FontPtr     pfont;
    unsigned long size;
    int         compression;
    xLbxFontInfo *fontinfo;
}           FontTagInfoRec, *FontTagInfoPtr;


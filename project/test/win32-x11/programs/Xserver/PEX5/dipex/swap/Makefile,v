PEX_SW_LIBNAME = libdiswapex.a

.c.obj:
	if exist $@@ $(RM) $@@
	$(CC) -c $*.c @@<<
	$(CFLAGS) $(_NOOP_)
<<

all:: diswapex.lib

XVAR0000 = $(OBJS)

diswapex.lib: $(XVAR0000:.o=.obj) $(EXTRALIBRARYDEPS)
	if exist $@@ $(RM) $@@
	$(AR) @@<<
-out:$@@ $(XVAR0000:.o=.obj)
<<
	-$(MKDIRHIER) $(BUILDLIBDIR:/=\)
	if exist $(BUILDLIBDIR:/=\)\$@@ $(RM) $(BUILDLIBDIR:/=\)\$@@
	cd $(BUILDLIBDIR:/=\) && $(LN) $(BUILDLIBTOP:/=\)\$(CURRENT_DIR:/=\)\$@@ .

lintlib:: llib-ldsw.ln

llib-ldsw.ln: $(SRCS) $(EXTRALIBRARYDEPS)
	if exist $@@ $(RM) $@@
	$(LINT) $(LINTLIBFLAG)dsw $(LINTFLAGS) $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

depend:: $(DEPEND).exe

$(DEPEND).exe:
 <<tmp.bat
 cd $(DEPENDSRC:/=\)
 $(MAKE)
<<

depend::
	$(DEPEND:/=\) @@<<
$(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)
<<

# ----------------------------------------------------------------------
# common rules for all Makefiles - do not edit

.c.i:
	if exist $@@ $(RM) $@@
	 $(CC) -E $(CFLAGS) $(_NOOP_) $*.c > $@@

emptyrule::

clean::
	@@$(MAKE) $(MFLAGS) -f <<
.SUFFIXES:

all: *.bak *.obj *.lib make.log
	!for %%i in ( Avoid.Error $$** ) do if exist %%i $(RM) %%i

Avoid.Error *.bak *.obj *.lib make.log :
<<

Makefile:: $(IMAKE).exe

$(IMAKE).exe:
 <<tmp.bat
 cd $(IMAKESRC:/=\)
 if exist Makefile $(MAKE) all
 if not exist Makefile $(MAKE) -f Makefile.ini BOOTSTRAPCFLAGS="$(BOOTSTRAPCFLAGS)"
<<

Makefile::
	-if exist Makefile.bak $(RM) Makefile.bak
	-@@if exist Makefile $(MV) Makefile Makefile.bak
	$(IMAKE_CMD) -DTOPDIR=$(TOP) -DCURDIR=$(CURRENT_DIR)

tags::
	$(TAGS) -w *.[ch]
	$(TAGS) -xw *.[ch] > TAGS

man_keywords::

# ----------------------------------------------------------------------
# empty rules for directories that do not have SUBDIRS - do not edit

install::
	@@echo "install in $(CURRENT_DIR) done"

install.man::
	@@echo "install.man in $(CURRENT_DIR) done"

install.linkkit::
	@@echo "install.linkkit in $(CURRENT_DIR) done"


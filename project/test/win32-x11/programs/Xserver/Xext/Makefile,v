all:: ext.lib

XVAR0000 = $(OBJS)

ext.lib: $(XVAR0000:.o=.obj) $(EXTRALIBRARYDEPS)
	if exist $@@ $(RM) $@@
	$(AR) @@<<
-out:$@@ $(XVAR0000:.o=.obj)
<<
	-$(MKDIRHIER) $(BUILDLIBDIR:/=\)
	if exist $(BUILDLIBDIR:/=\)\$@@ $(RM) $(BUILDLIBDIR:/=\)\$@@
	cd $(BUILDLIBDIR:/=\) && $(LN) $(BUILDLIBTOP:/=\)\$(CURRENT_DIR:/=\)\$@@ .

lintlib:: llib-lext.ln

llib-lext.ln: $(SRCS) $(EXTRALIBRARYDEPS)
	if exist $@@ $(RM) $@@
	$(LINT) $(LINTLIBFLAG)ext $(LINTFLAGS) $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

security.obj: security.c $(ICONFIGFILES)
	if exist $@@ $(RM) $@@
	$(CC) -c $*.c @@<<
	$(CFLAGS) $(POLICYFILEDEF)
<<

security.i: security.c $(ICONFIGFILES)
	if exist $@@ $(RM) $@@
	 $(CC) -E $(CFLAGS) $(POLICYFILEDEF) security.c > $@@

XVAR0001 = $(DESTDIR)$(SERVERCONFIGDIR)

install:: SecurityPolicy
	-$(MKDIRHIER) $(XVAR0001:/=\)
	$(INSTALL) $(INSTALLFLAGS) $(INSTDATFLAGS) SecurityPolicy $(XVAR0001:/=\)\\SecurityPolicy

depend:: $(DEPEND).exe

$(DEPEND).exe:
 <<tmp.bat
 cd $(DEPENDSRC:/=\)
 $(MAKE)
<<

depend::
	$(DEPEND:/=\) @@<<
$(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)
<<


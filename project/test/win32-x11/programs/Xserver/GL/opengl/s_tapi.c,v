#include "context.h"
#include "global.h"
#include "g_imfncs.h"

void GLAPIENTRY __glim_TexCoord1d(GLdouble x)
{
    __GL_SETUP();
    __GL_API_VAPI();
    gc->state.current.texture[0].x = x;
    gc->state.current.texture[0].y = __glZero;
    gc->state.current.texture[0].z = __glZero;
    gc->state.current.texture[0].w = __glOne;
}

void GLAPIENTRY __glim_TexCoord1f(GLfloat x)
{
    __GL_SETUP();
    __GL_API_VAPI();

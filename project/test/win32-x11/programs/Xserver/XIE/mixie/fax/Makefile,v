all:: DONE

DONE: $(XVAR0000:.o=.obj)
	if exist $@@ $(RM) $@@
	echo - > $@@

clean::
	if exist DONE $(RM) DONE

.c.obj:
	if exist $@@ $(RM) $@@
	$(CC) -c $*.c @@<<
	$(CFLAGS) $(_NOOP_)
<<

lintlib:: llib-lfax.ln


all:: os.lib

XVAR0000 = $(OBJS)

os.lib: $(XVAR0000:.o=.obj) $(EXTRALIBRARYDEPS)
	if exist $@@ $(RM) $@@
	$(AR) @@<<
-out:$@@ $(XVAR0000:.o=.obj)
<<
	-$(MKDIRHIER) $(BUILDLIBDIR:/=\)
	if exist $(BUILDLIBDIR:/=\)\$@@ $(RM) $(BUILDLIBDIR:/=\)\$@@
	cd $(BUILDLIBDIR:/=\) && $(LN) $(BUILDLIBTOP:/=\)\$(CURRENT_DIR:/=\)\$@@ .

lintlib:: llib-los.ln


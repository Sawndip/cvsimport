all:: dix.lib

XVAR0000 = $(OBJS)

dix.lib: $(XVAR0000:.o=.obj) $(EXTRALIBRARYDEPS)
	if exist $@@ $(RM) $@@
	$(AR) @@<<
-out:$@@ $(XVAR0000:.o=.obj)
<<
	-$(MKDIRHIER) $(BUILDLIBDIR:/=\)
	if exist $(BUILDLIBDIR:/=\)\$@@ $(RM) $(BUILDLIBDIR:/=\)\$@@
	cd $(BUILDLIBDIR:/=\) && $(LN) $(BUILDLIBTOP:/=\)\$(CURRENT_DIR:/=\)\$@@ .

lintlib:: llib-ldix.ln

llib-ldix.ln: $(SRCS) $(XPSRC) $(EXTRALIBRARYDEPS)
	if exist $@@ $(RM) $@@
	$(LINT) $(LINTLIBFLAG)dix $(LINTFLAGS) $(SRCS) $(XPSRC)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(XPSRC) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

globals.obj: globals.c $(ICONFIGFILES)
	if exist $@@ $(RM) $@@
	$(CC) -c $*.c @@<<
	$(CFLAGS) $(SITE_DEFINES)
<<

globals.i: globals.c $(ICONFIGFILES)
	if exist $@@ $(RM) $@@
	 $(CC) -E $(CFLAGS) $(SITE_DEFINES) globals.c > $@@

tables.obj: tables.c $(ICONFIGFILES)
	if exist $@@ $(RM) $@@
	$(CC) -c $*.c @@<<
	$(CFLAGS) $(K5DEFS)
<<

tables.i: tables.c $(ICONFIGFILES)
	if exist $@@ $(RM) $@@
	 $(CC) -E $(CFLAGS) $(K5DEFS) tables.c > $@@

dispatch.obj: dispatch.c $(ICONFIGFILES)
	if exist $@@ $(RM) $@@
	$(CC) -c $*.c @@<<
	$(CFLAGS) $(K5DEFS)
<<

dispatch.i: dispatch.c $(ICONFIGFILES)
	if exist $@@ $(RM) $@@
	 $(CC) -E $(CFLAGS) $(K5DEFS) dispatch.c > $@@


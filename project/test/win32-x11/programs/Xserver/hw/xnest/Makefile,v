XVAR0000 = ../../Xi/stubs.c

stubs.c: $(XVAR0000:/=\)
	if exist $@@ $(RM) $@@
	$(LN) $? $@@

includes:: stubs.c

depends:: stubs.c

clean::
	if exist stubs.c $(RM) stubs.c

Init.obj: Init.c $(ICONFIGFILES)
	if exist $@@ $(RM) $@@
	$(CC) -c $*.c @@<<
	$(CFLAGS) $(EXT_DEFINES)
<<

Init.i: Init.c $(ICONFIGFILES)
	if exist $@@ $(RM) $@@
	 $(CC) -E $(CFLAGS) $(EXT_DEFINES) Init.c > $@@

.c.obj:
	if exist $@@ $(RM) $@@
	$(CC) -c $*.c @@<<
	$(CFLAGS) $(_NOOP_)
<<

all:: xnest.lib

XVAR0001 = $(OBJS)

xnest.lib: $(XVAR0001:.o=.obj) $(EXTRALIBRARYDEPS)
	if exist $@@ $(RM) $@@
	$(AR) @@<<
-out:$@@ $(XVAR0001:.o=.obj)
<<
	-$(MKDIRHIER) $(BUILDLIBDIR:/=\)
	if exist $(BUILDLIBDIR:/=\)\$@@ $(RM) $(BUILDLIBDIR:/=\)\$@@
	cd $(BUILDLIBDIR:/=\) && $(LN) $(BUILDLIBTOP:/=\)\$(CURRENT_DIR:/=\)\$@@ .

XVAR0002 = $(DESTDIR)$(MANDIR)

install.man:: Xnest.man
	-$(MKDIRHIER) $(XVAR0002:/=\)
	$(INSTALL) $(INSTALLFLAGS) $(INSTMANFLAGS) Xnest.man $(XVAR0002:/=\)\\Xnest.$(MANSUFFIX)


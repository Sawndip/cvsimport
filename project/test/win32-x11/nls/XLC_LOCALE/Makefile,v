# $Xorg: Imakefile,v 1.3 2000/08/17 19:46:59 cpqbld Exp $

WCHAR32 = 0
TBLDIR = $(XLOCALEDIR)/tbl_data

all:: C.lt

XFAKEVAR = X
C.lt:: C
	if exist $@@ $(RM) $@@
	$(PREPROCESSCMD) -I. <<CppTmpFile.c @@<< >$@@
	# define $(XFAKEVAR)COMM #
	# include <C>
<<
	-DWCHAR32=$(WCHAR32)
<<

clean::
	@@$(MAKE) $(MFLAGS) -f <<
.SUFFIXES:

all: C.lt
	!for %%i in ( Avoid.Error $$** ) do if exist %%i $(RM) %%i

Avoid.Error C.lt:
<<

all:: C.lt
	-$(MKDIRHIER) $(BUILDLIBDIR:/=\)\locale\C
	if exist $(BUILDLIBDIR:/=\)\locale\C\XLC_LOCALE $(RM) $(BUILDLIBDIR:/=\)\locale\C\XLC_LOCALE
	cd $(BUILDLIBDIR:/=\)\locale\C && $(LN) $(BUILDLIBTOP:/=\)\..\..\$(CURRENT_DIR:/=\)\C.lt XLC_LOCALE

XVAR0000 = $(DESTDIR)$(XLOCALEDIR)/C

install:: C.lt
	-$(MKDIRHIER) $(XVAR0000:/=\)
	$(INSTALL) $(INSTALLFLAGS) $(INSTDATFLAGS) C.lt $(XVAR0000:/=\)\\XLC_LOCALE

all:: iso8859-1.lt


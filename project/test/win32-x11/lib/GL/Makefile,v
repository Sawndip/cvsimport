.c.obj:
	if exist $@@ $(RM) $@@
	$(CC) -c $*.c @@<<
	$(CFLAGS) $(_NOOP_)
<<

all:: GL.lib

XVAR0000 = $(OBJS)

GL.lib: $(XVAR0000:.o=.obj) $(EXTRALIBRARYDEPS)
	if exist $@@ $(RM) $@@
	$(AR) @@<<
-out:$@@ $(XVAR0000:.o=.obj)
<<
	-$(MKDIRHIER) $(BUILDLIBDIR:/=\)
	if exist $(BUILDLIBDIR:/=\)\$@@ $(RM) $(BUILDLIBDIR:/=\)\$@@
	cd $(BUILDLIBDIR:/=\) && $(LN) $(BUILDLIBTOP:/=\)\$(CURRENT_DIR:/=\)\$@@ .

XVAR0001 = $(SUBDIRS)
XVAR0002 = $$

all::
	@@$(MAKE) -A $(MFLAGS) -f <<
.SUFFIXES:

all: $(XVAR0001:/=\)

Avoid.Error $(XVAR0001:/=\):
 cd $(XVAR0002)@@
 @@echo "making" all in $(CURRENT_DIR)/$(XVAR0002)@@...
 $(MAKE) $(MFLAGS) CDEBUGFLAGS="$(CDEBUGFLAGS)" all
 cd ..
<<

XVAR0003 = $(SUBDIRS)
XVAR0004 = $$

depend::
	@@$(MAKE) -A $(MFLAGS) -f <<
.SUFFIXES:

all: $(XVAR0003:/=\)

Avoid.Error $(XVAR0003:/=\):
 cd $(XVAR0004)@@
 @@echo "depending" in $(CURRENT_DIR)/$(XVAR0004)@@...
 $(MAKE) $(MFLAGS)  depend
 cd ..
<<

depend:: $(DEPEND).exe

$(DEPEND).exe:
 <<tmp.bat
 cd $(DEPENDSRC:/=\)
 $(MAKE)
<<

depend::
	$(DEPEND:/=\) @@<<
$(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)
<<

# ----------------------------------------------------------------------
# common rules for all Makefiles - do not edit

.c.i:
	if exist $@@ $(RM) $@@
	 $(CC) -E $(CFLAGS) $(_NOOP_) $*.c > $@@

emptyrule::

clean::
	@@$(MAKE) $(MFLAGS) -f <<
.SUFFIXES:

all: *.bak *.obj *.lib make.log
	!for %%i in ( Avoid.Error $$** ) do if exist %%i $(RM) %%i

Avoid.Error *.bak *.obj *.lib make.log :
<<

Makefile:: $(IMAKE).exe

$(IMAKE).exe:
 <<tmp.bat
 cd $(IMAKESRC:/=\)
 if exist Makefile $(MAKE) all
 if not exist Makefile $(MAKE) -f Makefile.ini BOOTSTRAPCFLAGS="$(BOOTSTRAPCFLAGS)"
<<

Makefile::
	-if exist Makefile.bak $(RM) Makefile.bak
	-@@if exist Makefile $(MV) Makefile Makefile.bak
	$(IMAKE_CMD) -DTOPDIR=$(TOP) -DCURDIR=$(CURRENT_DIR)

tags::
	$(TAGS) -w *.[ch]
	$(TAGS) -xw *.[ch] > TAGS

man_keywords::


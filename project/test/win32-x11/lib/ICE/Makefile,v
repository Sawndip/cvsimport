	$(_NULLCMD_)
	$(_NULLCMD_)
	$(_NULLCMD_)

all:: $(LIBNAME).dll

XFAKEVAR = X
$(LIBNAME).def:: $(LIBNAME)-def.cpp $(ICONFIGFILES)
 if exist $@@ $(RM) $@@
 $(PREPROCESSCMD) -I. <<CppTmpFile.c @@<< >$@@
 # define $(XFAKEVAR)COMM #
 # include <$(LIBNAME)-def.cpp>
<<
	-DLIBRARY_VERSION=$(SOICEREV)
<<

clean::
	@@$(MAKE) $(MFLAGS) -f <<
.SUFFIXES:

all: $(LIBNAME).def
	!for %%i in ( Avoid.Error $$** ) do if exist %%i $(RM) %%i

Avoid.Error $(LIBNAME).def:
<<

XVAR0000 = $(OBJS)

$(LIBNAME).lib: $(XVAR0000:.o=.obj) $(LIBNAME).def $(EXTRALIBRARYDEPS)
 if exist $@@ $(RM) $@@
 $(AR) @@<<
-out:$@@ -def:$(LIBNAME).def $(XVAR0000:.o=.obj)
<<
	-$(MKDIRHIER) $(BUILDLIBDIR:/=\)
	if exist $(BUILDLIBDIR:/=\)\$@@ $(RM) $(BUILDLIBDIR:/=\)\$@@
	cd $(BUILDLIBDIR:/=\) && $(LN) $(BUILDLIBTOP:/=\)\$(CURRENT_DIR:/=\)\$@@ .

$(LIBNAME).dll: $(LIBNAME).lib $(EXTRALIBRARYDEPS)
 if exist $(LIBNAME).dll $(RM) $(LIBNAME).dll
 $(LD) @@<<
$(SHLIBLDFLAGS) -out:$(LIBNAME).dll $(LIBNAME).exp $(XVAR0000:.o=.obj) $(REQUIREDLIBS) $(EXTRA_LIBRARIES)
<<
	-$(MKDIRHIER) $(BUILDBINDIR:/=\)
	if exist $(BUILDBINDIR:/=\)\$(LIBNAME).dll $(RM) $(BUILDBINDIR:/=\)\$(LIBNAME).dll
	cd $(BUILDBINDIR:/=\) && $(LN) $(BUILDBINTOP:/=\)\$(CURRENT_DIR:/=\)\$(LIBNAME).dll .

clean::
	if exist $(LIBNAME).lib $(RM) $(LIBNAME).lib
	if exist $(LIBNAME).dll $(RM) $(LIBNAME).dll


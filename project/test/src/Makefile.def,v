	stage_cflags="$(STAGE2_CFLAGS)" ;
	stage_libcflags="$(STAGE2_LIBCFLAGS)" ; };
bootstrap_stage = {
	id=b2g0 ; prev=1 ;
	bootstrap_target=bootstrap2-debug ;
	stage_configure_flags="@@stage2_werror_flag@@" ;
	stage_cflags="$(STAGE2_CFLAGS) -g0" ;
	stage_libcflags="$(STAGE2_LIBCFLAGS) -g0" ; };

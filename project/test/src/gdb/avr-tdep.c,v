struct avr_unwind_cache *
avr_frame_unwind_cache (struct frame_info *next_frame,
                        void **this_prologue_cache)
d928 1
a928 5
  CORE_ADDR pc;
  ULONGEST prev_sp;
  ULONGEST this_base;

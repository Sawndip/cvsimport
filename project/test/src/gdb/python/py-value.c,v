	    {
	      /* Check the value's type is something that can be accessed via
		 a subscript.  */
	      struct type *type;
	      tmp = coerce_ref (tmp);
	      type = check_typedef (value_type (tmp));
	      if (TYPE_CODE (type) != TYPE_CODE_ARRAY
		  && TYPE_CODE (type) != TYPE_CODE_PTR)
		  error( _("Cannot subscript requested type"));
	      else

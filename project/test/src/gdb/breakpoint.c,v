		     CATCH_TEMPORARY);
  add_catch_command ("unload", _("\
Catch library unloads.\n\
With an argument, catch only unloads of that library."),
		     catch_unload_command_1,
		     CATCH_PERMANENT,
		     CATCH_TEMPORARY);

static void
create_longjmp_breakpoint (char *func_name)
{
  struct minimal_symbol *m;

  m = lookup_minimal_symbol_text (func_name, NULL);
  if (m == NULL)
    return;
  set_momentary_breakpoint_at_pc (SYMBOL_VALUE_ADDRESS (m), bp_longjmp);
  update_global_location_list (1);
}


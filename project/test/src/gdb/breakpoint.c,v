	      || b->type == bp_longjmp_master
	      || b->type == bp_std_terminate_master
	      || b->type == bp_exception_master)
	    /* We do not stop for these.  */
	    bs->stop = 0;
	  else
	    bpstat_check_breakpoint_conditions (bs, ptid);

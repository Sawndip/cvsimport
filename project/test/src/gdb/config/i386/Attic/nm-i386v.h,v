   to get the offset in the core file of the register values.  */

#define REGISTER_U_ADDR(addr, blockend, regnum) \
  (addr) = register_u_addr ((blockend), (regnum))
extern CORE_ADDR register_u_addr (CORE_ADDR blockend, int regnum);



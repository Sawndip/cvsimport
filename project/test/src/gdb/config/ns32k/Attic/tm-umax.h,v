#define SKIP_PROLOGUE(pc)   				\
{ register unsigned char op = read_memory_integer (pc, 1);	\
  if (op == 0x82) { op = read_memory_integer (pc+2,1);  \
  		    if ((op & 0x80) == 0) pc += 3;	\
		    else if ((op & 0xc0) == 0x80) pc += 4;	\
		    else pc += 6;			\
		   }					\
}

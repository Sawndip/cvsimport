#ifndef NM_NBSD_H
#define NM_NBSD_H

/* Get generic NetBSD native definitions. */
#include "config/nm-nbsd.h"

#define REGISTER_U_ADDR(addr, blockend, regno) \
	(addr) = ns32k_register_u_addr ((blockend),(regno));

extern int ns32k_register_u_addr (int, int);

#endif /* NM_NBSD_H */

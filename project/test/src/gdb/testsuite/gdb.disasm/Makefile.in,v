.PHONY: all clean mostlyclean distclean realclean

all info install-info dvi install uninstall installcheck check:
	@@echo "Nothing to be done for $@@..."

clean mostlyclean:
	-rm -f *.o *.diff *~ *.bad core h8300s hppa mn10300 sh3

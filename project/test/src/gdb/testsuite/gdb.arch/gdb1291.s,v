	sts.l	pr,@@-r15
	add	#-128,r15
	add	#-128,r15
	mov	r15,r14
	mov.w	.L8,r7
	add	r7,r14
	mov	r14,r15
	lds.l	@@r15+,pr
	mov.l	@@r15+,r14
	rts	

	mov.l	r14,@@-r15
	sts.l	pr,@@-r15
	mov	r15,r14
	mov.l	.L2,r1
	mov	r1,r4
	mov.l	.L3,r1
	jsr	@@r1
	nop
	mov.l	.L4,r1
	jsr	@@r1
	nop
	mov.l	.L5,r1
	jsr	@@r1

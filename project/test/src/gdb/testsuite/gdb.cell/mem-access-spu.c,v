#include <stdio.h>

int
main (unsigned long long speid, unsigned long long argp,
      unsigned long long envp)
{
  int i;
  static int test_var;

  printf ("Hello World! from spu\n");

  test_var = 5;
  i = 5; /* Marker SPUEA */
  printf ("i = %d\n", i);

  return 0;
}

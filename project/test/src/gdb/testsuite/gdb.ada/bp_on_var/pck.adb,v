package body Pck is
   My_Hidden_Variable : Integer := 0;
   procedure Do_Something (I : in out Integer) is
   begin
      if My_Hidden_Variable > 10 then
         My_Hidden_Variable := 0;
      end if;
      I := I + My_Hidden_Variable;
      My_Hidden_Variable := My_Hidden_Variable + 1;
   end Do_Something;
end Pck;

@



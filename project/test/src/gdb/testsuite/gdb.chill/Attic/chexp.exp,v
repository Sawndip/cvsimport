# OBSOLETE     gdb_test "p .44 > .45" " = FALSE"
# OBSOLETE     gdb_test "p 0.44 < 0.45" " = TRUE"
# OBSOLETE     gdb_test "p 0.44 > 0.45" " = FALSE"
# OBSOLETE     gdb_test "p 44. < 45." " = TRUE"

# OBSOLETE     gdb_test "p 10.0E+20 < 10.0E+21" " = TRUE"
# OBSOLETE     gdb_test "p 10.0E+20 > 10.0E+21" " = FALSE"
# OBSOLETE     gdb_test "p 10.0e+20 < 10.0e+21" " = TRUE"
# OBSOLETE     gdb_test "p 10.0e+20 > 10.0e+21" " = FALSE"
# OBSOLETE     gdb_test "p 10.0D-11 < 10.0D-10" " = TRUE"

gdb_test "whatis (@@code enum misordered) v_misordered" \
	"type = @@code enum misordered" \
	"(@@code enum misordered)"
gdb_test "whatis (enum misordered @@code) v_misordered" \
	"type = @@code enum misordered" \
	"(enum misordered @@code)"

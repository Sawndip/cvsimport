	"type = @@code struct t_struct${ws}\\*${ws}@@code" \
	"(struct t_struct @@code * @@code)"
gdb_test "whatis (union t_union @@code * @@code) &v_union" \
	"type = @@code union t_union${ws}\\*${ws}@@code" \
	"(union t_union @@code * @@code)"


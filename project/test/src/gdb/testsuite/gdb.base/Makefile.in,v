@VPATH = @@srcdir@@
srcdir = @@srcdir@@

EXECUTABLES = a2-run advance all-types annota1 annota1-watch_thread_num \
	annota3 anon args arrayidx async attach attach-pie-misread \
	attach2 auxv bang\! bfp-test bigcore bitfields bitfields2 \

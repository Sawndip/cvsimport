text
@d58 2
a59 11
set escapedfilename [string_to_regexp ${gcorefile}]

set test "save a corefile"
gdb_test_multiple "gcore ${gcorefile}" $test {
    -re "Saved corefile ${escapedfilename}\r\n$gdb_prompt $" {
	pass $test
    }
    -re "Can't create a corefile\r\n$gdb_prompt $" {
	unsupported $test
	return -1
    }
@


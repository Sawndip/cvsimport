1.1.1.2
log
@import gdb-1999-06-28 snapshot
@
text
@a4 1
#include <stdlib.h>
a5 3
#ifdef PROTOTYPES
int callee (int x)
#else
a7 1
#endif
d13 1
a13 1
int main()
a26 1
    return 0;
@


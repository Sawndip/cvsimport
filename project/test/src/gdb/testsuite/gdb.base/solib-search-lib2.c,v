lib2_func2 (void)
{
  lib1_func3 ();
}

/* Make the relative address of func4 different b/w the "wrong" and "right"
   versions of the library" to further ensure backtrace doesn't work with
   the "wrong" version.  */

#ifdef RIGHT

void
lib2_spacer (void)
{
}

#endif

void
lib2_func4 (void)
{
  break_here ();
}

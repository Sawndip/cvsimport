   along with this program.  If not, see <http://www.gnu.org/licenses/>.  */

template<class T> T add(T v1, T v2);

void add1()
{
  unsigned char c;
  int i;
  float f;
  
  c = 'b';
  i = 3;
  f = 6.5;

  c = add(c, c);
  i = add(i, i);
  f = add(f, f);
}

/* A copy of some classes in derivation.cc so that we can test symbol lookup
   in other CUs.  */

class A2 {
public:
    typedef int value_type;
    value_type a;

    A2()
    {
        a=1;
    }
};

class D2 : public A2 {
public:
    value_type d;

    D2()
    {
        d=7;
    }
};

void
foo2 ()
{
  D2 d2_instance;
  d2_instance.a = 42;
  d2_instance.d = 43;
}
@



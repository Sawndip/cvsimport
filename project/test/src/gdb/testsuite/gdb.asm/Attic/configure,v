archinc=common.inc
case ${target} in
*arm-*-*) archinc=arm.inc ;;
xscale-*-*) archinc=arm.inc ;;
d10v-*-*) archinc=d10v.inc ;;
s390-*-*) archinc=s390.inc ;;
i[3456]86*) archinc=i386.inc ;;
m32r*-*) archinc=m32r.inc ;;
powerpc*-*) archinc=powerpc.inc ;;
sparc-*-*) archinc=sparc.inc ;;

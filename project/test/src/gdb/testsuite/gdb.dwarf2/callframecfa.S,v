/* This was compiled from a trivial program just to test the
   DW_OP_call_frame_cfa operator:

    int func (int arg) {
      return arg + 23;
    }

    int main(int argc, char *argv[]) {
      func (77);
    }
*/

	.file	"q.c"
	.section	.debug_abbrev,"",@@progbits
.Ldebug_abbrev0:
	.section	.debug_info,"",@@progbits
.Ldebug_info0:
	.section	.debug_line,"",@@progbits
.Ldebug_line0:
	.text
.Ltext0:
.globl func
	.type	func, @@function
func:
.LFB0:
	.file 1 "q.c"
	.loc 1 2 0
	.cfi_startproc
	pushl	%ebp
	.cfi_def_cfa_offset 8
	movl	%esp, %ebp
	.cfi_offset 5, -8
	.cfi_def_cfa_register 5
	.loc 1 3 0
	movl	8(%ebp), %eax
	addl	$23, %eax
	.loc 1 4 0
	popl	%ebp
	.cfi_restore 5
	.cfi_def_cfa 4, 4
	ret
	.cfi_endproc
.LFE0:
	.size	func, .-func
.globl _start
	.type	_start, @@function
_start:
.LFB1:
	.loc 1 6 0
	.cfi_startproc
	pushl	%ebp
	.cfi_def_cfa_offset 8
	movl	%esp, %ebp
	.cfi_offset 5, -8
	.cfi_def_cfa_register 5
	subl	$4, %esp
	.loc 1 7 0
	movl	$77, (%esp)
	call	func
	.loc 1 8 0
	leave
	.cfi_restore 5
	.cfi_def_cfa 4, 4
	ret
	.cfi_endproc
.LFE1:
	.size	_start, .-_start

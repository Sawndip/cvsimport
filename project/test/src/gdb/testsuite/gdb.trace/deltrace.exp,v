send_gdb "delete tracepoints\n"
gdb_expect 30 {
    -re "Delete all tracepoints.*y or n.*$" {
	send_gdb "y\n"
	gdb_expect 30 {
	    -re "$gdb_prompt $" {
		pass "3.1b: delete all tracepoints"
	    }
	    timeout { fail "3.1b: delete all tracepoints (timeout)" }
	}
    }

fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $gl_cv_header_working_stdint_h" >&5
$as_echo "$gl_cv_header_working_stdint_h" >&6; }
  fi
  if test "$gl_cv_header_working_stdint_h" = yes; then
    STDINT_H=
  else
            for ac_header in sys/inttypes.h sys/bitypes.h
do :

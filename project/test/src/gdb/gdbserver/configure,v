  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_compile "$LINENO"; then :
  bfd_cv_have_sys_procfs_type_lwpid_t=yes
else
  bfd_cv_have_sys_procfs_type_lwpid_t=no

fi
rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
fi


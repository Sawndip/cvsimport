a77 6
static int
hppa_hpux_cannot_fetch_register (int regnum)
{
  gdb_assert (regnum >= 0 && regnum < NUM_REGS);
  return (regnum >= ARRAY_SIZE(hppa_hpux_save_state_offset));
}

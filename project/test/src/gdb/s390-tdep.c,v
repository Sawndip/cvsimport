  /* instruction sequence for s390 call dummy is as follows
     bras %r1,.+8      ; 0xA7150004   
     long basraddr     ; 0x00000000
     l    %r1,0(%r1)   ; 0x58101000
     basr %r14,%r1     ; 0x0DE1

	  break;
	case DW_CFA_same_value:
	  reg = LEB ();
	  frame_need_space (fc, reg);
	  fc->col_type[reg] = DW_CFA_undefined;
	  break;
	case DW_CFA_register:
	  reg = LEB ();
	  LEB ();
	  frame_need_space (fc, reg);

a1111 2
static gdb_byte *skip_leb128 (bfd *, gdb_byte *);

d3662 1
d5041 2
d5121 1
a5121 1
	  info_ptr = skip_leb128 (abfd, info_ptr);
a12281 16
/* Return a pointer to just past the end of an LEB128 number in BUF.  */

static gdb_byte *
skip_leb128 (bfd *abfd, gdb_byte *buf)
{
  int byte;

  while (1)
    {
      byte = bfd_get_8 (abfd, buf);
      buf++;
      if ((byte & 128) == 0)
	return buf;
    }
}


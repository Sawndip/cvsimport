/* Release one cached compilation unit, CU.  */

static void
free_one_comp_unit (struct dwarf2_cu *cu)
{
  obstack_free (&cu->partial_die_obstack, NULL);
  if (cu->dies)
    free_die_list (cu->dies);
  xfree (cu);
}


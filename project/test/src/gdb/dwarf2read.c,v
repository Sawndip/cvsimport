d3690 1
a3690 28
  if (die->child != NULL && ! die_is_declaration (die, cu))
    {
      struct die_info *child_die;

      child_die = die->child;

      while (child_die && child_die->tag)
	{
	  if (child_die->tag == DW_TAG_member
	      || child_die->tag == DW_TAG_variable
	      || child_die->tag == DW_TAG_inheritance)
	    {

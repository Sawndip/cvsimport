d2237 5
d2244 1
a2244 12
    {
      high = DW_ADDR (attr);
      attr = dwarf_attr (die, DW_AT_low_pc);
      if (attr)
	low = DW_ADDR (attr);
      else
	/* Found high w/o low attribute.  */
	return 0;

      /* Found consecutive range of addresses.  */

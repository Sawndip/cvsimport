a3918 52
  struct objfile *objfile = this_cu->objfile;
  bfd *abfd = objfile->obfd;
  gdb_byte *info_ptr;
  struct die_info *comp_unit_die;
  struct dwarf2_cu *cu;
  struct cleanup *free_abbrevs_cleanup, *free_cu_cleanup = NULL;
  int has_children;
  struct die_reader_specs reader_specs;

	      -e 's|@@''HAVE_MBSINIT''@@|$(HAVE_MBSINIT)|g' \
	      -e 's|@@''HAVE_MBRTOWC''@@|$(HAVE_MBRTOWC)|g' \
	      -e 's|@@''HAVE_MBRLEN''@@|$(HAVE_MBRLEN)|g' \

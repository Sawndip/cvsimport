                    psr->symtab = s;
                    psr->symbol = sym;
                    psr->msymbol = NULL;
                    psr->next = NULL;
                    if (tail == NULL)
                      {
                        sr = psr;
                        old_chain = make_cleanup ((make_cleanup_func) 
                                                  free_search_symbols, sr);
                      }
                    else
                      tail->next = psr;
                    tail = psr;
                  }
              }
          }

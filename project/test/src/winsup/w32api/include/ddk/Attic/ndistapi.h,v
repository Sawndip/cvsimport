  /*IN*/ ULONG  ulLineID;
  /*IN*/ ULONG  ulAddressID;
  /*OUT*/ ULONG  ulFlags;
  /*OUT*/ LINE_ADDRESS_CAPS  LineAddressCaps;
} CO_TAPI_ADDRESS_CAPS, *PCO_TAPI_ADDRESS_CAPS;


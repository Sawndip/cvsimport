  if test "$cross_compiling" = yes; then
  case "${host}" in
	 *-*-cygwin*) echo "$ac_t""no" 1>&6 ;;
	 *) { echo "configure: error: Expect can't be cross compiled" 1>&2; exit 1; } ;;
	 esac


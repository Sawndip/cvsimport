fi
if test -z "$ac_cv_prog_AR_FOR_TARGET" ; then
  if test $build = $target ; then
    ncn_cv_AR_FOR_TARGET=$AR_FOR_TARGET
    # Extract the first word of "ar", so it can be a program name with args.

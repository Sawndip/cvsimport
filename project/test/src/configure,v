  echo "$ac_t""no" 1>&6
fi
 
    OBJCOPY=$ncn_cv_OBJCOPY
  else
    OBJCOPY="${ncn_tool_prefix}objcopy"
  fi
else
  OBJCOPY="$ac_cv_prog_OBJCOPY"

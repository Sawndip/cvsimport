#include <mpc.h>
int
main ()
{

    mpc_t n;
    mpc_init2 (n, 53);
    mpc_set_ui_ui (n, 1, 1, MPC_RNDNN);
    mpc_sin (n, n, MPC_RNDNN);
    mpc_cos (n, n, MPC_RNDNN);

#include "sysdep.h"
#include "bfd.h"
#include "libbfd.h"

const bfd_arch_info_type bfd_m9s12x_arch =
{
    16,	/* 16 bits in a word.  */
    32,	/* 16 bits in an address.  */
    8,	/* 8 bits in a byte.  */
    bfd_arch_m9s12x,
    0,
    "m9s12x",
    "m9s12x",
    4, /* Section alignment power.  */
    TRUE,
    bfd_default_compatible,
    bfd_default_scan,
    bfd_arch_default_fill,
    0,
};


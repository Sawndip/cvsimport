#include "sysdep.h"
#include "bfd.h"
#include "libbfd.h"

const bfd_arch_info_type bfd_xgate_arch =
{
    16, /* 16 bits in a word.  */
    32, /* 32 bits used as 16 bit address and PPAGE value.  */
    8,  /* 8 bits in a byte.  */
    bfd_arch_xgate,
    bfd_mach_xgate,
    "xgate",
    "xgate",
    4, 	/* Section alignment power.  */
    TRUE,
    bfd_default_compatible,
    bfd_default_scan,
    bfd_arch_default_fill,
    0,
};
@

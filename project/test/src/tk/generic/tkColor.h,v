/* CYGNUS LOCAL.  */

/* A linked list of GC structures.  */

typedef struct TkGCList {
    /* Next item on list.  */
    struct TkGCList *next;
    /* The display for the GC.  */
    Display *display;
    /* The GC.  */
    GC gc;
    /* GCForeground or GCBackground.  */
    unsigned long mask;
} TkGCList;

/* END CYGNUS LOCAL */


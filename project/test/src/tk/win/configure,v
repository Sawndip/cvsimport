EXESUFFIX!$EXESUFFIX$ac_delim
LIBRARIES!$LIBRARIES$ac_delim
MAKE_LIB!$MAKE_LIB$ac_delim
POST_MAKE_LIB!$POST_MAKE_LIB$ac_delim

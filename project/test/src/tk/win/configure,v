    ;;
  esac

        TK_STUB_LIB_SPEC="-L${dirname} ${TK_STUB_LIB_FLAG}"
      fi
    ;;
    *)
      TK_STUB_LIB_SPEC="-L${exec_prefix}/lib ${TK_STUB_LIB_FLAG}"
    ;;
  esac



  
  val="`pwd`/${TK_STUB_LIB_FILE}"


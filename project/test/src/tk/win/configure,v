      TK_STUB_LIB_SPEC=$val
    ;;
  esac

      else
        
  val=${exec_prefix}/lib

  if test "$val" = "" ; then
    { echo "configure: error: Empty value for variable dirname" 1>&2; exit 1; }
  fi


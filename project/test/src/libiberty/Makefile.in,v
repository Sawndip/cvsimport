text
@a54 3
# Some compilers can't handle cc -c blah.c -o foo/blah.o.
OUTPUT_OPTION = @@OUTPUT_OPTION@@

d120 1
a120 1
	$(COMPILE.c) $< $(OUTPUT_OPTION)
d259 3

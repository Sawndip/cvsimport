    "insque.$ac_objext"   | \
  *" insque.$ac_objext"   | \
    "insque.$ac_objext "* | \
  *" insque.$ac_objext "* ) ;;
  *) LIBOBJS="$LIBOBJS insque.$ac_objext" ;;

# Look for a 64-bit type.
echo "$as_me:$LINENO: checking for a 64-bit type" >&5
echo $ECHO_N "checking for a 64-bit type... $ECHO_C" >&6
if test "${liberty_cv_uint64+set}" = set; then
  echo $ECHO_N "(cached) $ECHO_C" >&6

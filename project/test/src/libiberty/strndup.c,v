#define size_t unsigned long
#endif
d36 3
a38 3
extern size_t	strlen PARAMS ((const char*));
extern PTR	malloc PARAMS ((size_t));
extern PTR	memcpy PARAMS ((PTR, const PTR, size_t));
d41 1
a41 3
strndup(s, n)
     const char *s;
     size_t n;
@


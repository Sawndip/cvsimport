  gconfig.h $(INCDIR)/bin-bugs.h source.h search_list.h \
  symtab.h ../bfd/bfd.h corefile.h gmon_io.h gmon.h gmon_out.h \
  $(INCDIR)/libiberty.h sym_ids.h

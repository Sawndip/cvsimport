#line 4285 "configure"
#include "confdefs.h"
#include <stdio.h>
#include <stdlib.h>
#if HAVE_SYS_GMON_OUT_H
#include <sys/gmon_out.h>
#endif
main()
{
#if HAVE_SYS_GMON_OUT_H
  struct gmon_cg_arc_record arc;
  FILE *f=fopen("conftestval", "w");
  if (!f) exit(1);
  fprintf(f, "%d\n", sizeof(arc.from_pc));
  exit(0);
#else
  FILE *f=fopen("conftestval", "w");
  if (!f) exit(1);
  fprintf(f, "%d\n", (int) sizeof(char *));
  exit(1);
#endif
}
EOF

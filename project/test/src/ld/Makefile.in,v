		| grep 'target.*spu' >/dev/null 2>/dev/null; then \
	  cpp -DOVLY_IRQ_SAVE $(srcdir)/emultempl/spu_ovl.S spu_ovl.s; \
	  ../gas/as-new -o spu_ovl.@@OBJEXT@@ spu_ovl.s; \
	  ../binutils/bin2c <spu_ovl.@@OBJEXT@@ >$@@; \
	fi

@@ENABLE_PLUGINS_TRUE@@noinst_LTLIBRARIES = libldtestplug.la
@@ENABLE_PLUGINS_TRUE@@libldtestplug_la_SOURCES = testplug.c
@@ENABLE_PLUGINS_TRUE@@libldtestplug_la_CFLAGS = -g -O2

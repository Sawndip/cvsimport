set build_tests {
  {"Build libentry.a"
   "" "" ""
   {entry.s} {} "libentry.a"}
}

run_ld_link_tests $build_tests

set test_list [lsort [glob -nocomplain $srcdir/$subdir/entry*.d]]
foreach t $test_list {
    # We need to strip the ".d", but can leave the dirname.
    verbose [file rootname $t]
    run_dump_test [file rootname $t]
}
@



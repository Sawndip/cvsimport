1.1.1.1.2.1
log
@Use ${CONSTRUCTING...} to enclose constructors and destructors.
@
text
@a0 8
TORS="
    ___ctors = . ;
    *(.ctors)
    ___ctors_end = . ;
    ___dtors = . ;
    *(.dtors)
    ___dtors_end = . ;"

d11 2
a12 2
	  *(.text) 				
   	  ${RELOCATING+ _etext = . ; }
d17 8
a24 4
	  *(.rdata); 
	  *(.strings)
	  
	  ${CONSTRUCTING+${TORS}}
d29 2
a30 2
	  *(.data)
	  ${RELOCATING+ _edata = . ; }
d35 4
a38 4
	  ${RELOCATING+ __start_bss = . ; }
	  *(.bss)
	  *(COMMON)
	  ${RELOCATING+ _end = . ;  }
d43 2
a44 2
	  ${RELOCATING+ _stack = . ; }
	  *(.stack)
d47 8
a54 9
.stab  0 ${RELOCATING+(NOLOAD)} : 
	{
          [ .stab ]
	}
	
.stabstr  0 ${RELOCATING+(NOLOAD)} :
	{
	  [ .stabstr ]
	}
@



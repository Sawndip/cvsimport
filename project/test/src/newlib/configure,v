  enableval=$enable_newlib_wide_orient; case "${enableval}" in
   yes) newlib_wide_orient=yes ;;
   no)  newlib_wide_orient=no ;;
   *) as_fn_error $? "bad value ${enableval} for newlib-wide-orient" "$LINENO" 5 ;;
 esac

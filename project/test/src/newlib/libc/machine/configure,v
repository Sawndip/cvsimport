as_func_success () {
  as_func_return 0
}
as_func_failure () {
  as_func_return 1
}
as_func_ret_success () {
  return 0
}
as_func_ret_failure () {
  return 1
}

exitcode=0
if as_func_success; then
  :
d387 1
a387 2
  exitcode=1
  echo as_func_success failed.
d390 2
a391 7
if as_func_failure; then
  exitcode=1
  echo as_func_failure succeeded.
fi


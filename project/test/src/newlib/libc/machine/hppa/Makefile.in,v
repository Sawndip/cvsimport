	$(CCAS) $(DEFS) $(DEFAULT_INCLUDES) $(INCLUDES) $(AM_CPPFLAGS) $(CPPFLAGS) $(lib_a_CCASFLAGS) $(CCASFLAGS) -c -o lib_a-strncat.obj `if test -f 'strncat.S'; then $(CYGPATH_W) 'strncat.S'; else $(CYGPATH_W) '$(srcdir)/strncat.S'; fi`


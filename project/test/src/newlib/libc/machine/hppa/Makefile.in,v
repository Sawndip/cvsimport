lib_a-strncat.o: strncat.S
	$(CCAS) $(DEFS) $(DEFAULT_INCLUDES) $(INCLUDES) $(AM_CPPFLAGS) $(CPPFLAGS) $(lib_a_CCASFLAGS) $(CCASFLAGS) -c -o lib_a-strncat.o `test -f 'strncat.S' || echo '$(srcdir)/'`strncat.S


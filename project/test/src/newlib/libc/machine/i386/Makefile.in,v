	-rm -f *.$(OBJEXT)

distclean-compile:
	-rm -f *.tab.c

.S.o:
	$(CPPASCOMPILE) -c -o $@@ $<

.S.obj:
	$(CPPASCOMPILE) -c -o $@@ `$(CYGPATH_W) '$<'`

.S.lo:
	$(LTCPPASCOMPILE) -c -o $@@ $<

lib_a-memchr.o: memchr.S
	$(CCAS) $(DEFS) $(DEFAULT_INCLUDES) $(INCLUDES) $(AM_CPPFLAGS) $(CPPFLAGS) $(lib_a_CCASFLAGS) $(CCASFLAGS) -c -o lib_a-memchr.o `test -f 'memchr.S' || echo '$(srcdir)/'`memchr.S


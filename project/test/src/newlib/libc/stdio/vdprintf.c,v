_DEFUN(_vdprintf_r, (ptr, fd, format, ap),
       struct _reent *ptr _AND
       int fd _AND
       const char *format _AND
       va_list ap)
d24 8
a31 10
  char *p;
  int n;

  _REENT_SMALL_CHECK_INIT (ptr);
  n = _vasprintf_r (ptr, &p, format, ap);
  if (n == -1)
    return -1;
  n = _write_r (ptr, fd, p, n);
  _free_r (ptr, p);
  return n;

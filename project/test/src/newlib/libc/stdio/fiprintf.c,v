_DEFUN(_fiprintf_r, (ptr, fp, fmt),
       struct _reent *ptr _AND
       FILE * fp _AND
       const char *fmt _DOTS)
{
  int ret;
  va_list ap;

  va_start (ap, fmt);
  ret = _vfiprintf_r (ptr, fp, fmt, ap);
  va_end (ap);
  return ret;
}


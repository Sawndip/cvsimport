/* This code was copied from sprintf.c */
/* doc in sprintf.c */

#include <_ansi.h>
#include <reent.h>
#include <stdio.h>
#include <stdarg.h>
#include <limits.h>
#include "local.h"

int
_DEFUN(_asprintf_r, (ptr, strp, fmt),
       struct _reent *ptr _AND
       char **__restrict strp        _AND
       const char *__restrict fmt _DOTS)
{
  int ret;
  va_list ap;
  FILE f;

  /* mark a zero-length reallocatable buffer */
  f._flags = __SWR | __SSTR | __SMBF;
  f._bf._base = f._p = NULL;
  f._bf._size = f._w = 0;
  f._file = -1;  /* No file. */
  va_start (ap, fmt);
  ret = _svfprintf_r (ptr, &f, fmt, ap);
  va_end (ap);
  if (ret >= 0)
    {
      *f._p = 0;
      *strp = (char *) f._bf._base;
    }
  return (ret);
}

#ifdef _NANO_FORMATTED_IO
int
_EXFUN(_asiprintf_r, (struct _reent *, char **, const char *, ...)
       _ATTRIBUTE ((__alias__("_asprintf_r"))));
#endif

#ifndef _REENT_ONLY

int
_DEFUN(asprintf, (strp, fmt),
       char **__restrict strp _AND
       const char *__restrict fmt _DOTS)
{
  int ret;
  va_list ap;
  FILE f;

  /* mark a zero-length reallocatable buffer */
  f._flags = __SWR | __SSTR | __SMBF;
  f._bf._base = f._p = NULL;
  f._bf._size = f._w = 0;
  f._file = -1;  /* No file. */
  va_start (ap, fmt);
  ret = _svfprintf_r (_REENT, &f, fmt, ap);
  va_end (ap);
  if (ret >= 0)
    {
      *f._p = 0;
      *strp = (char *) f._bf._base;
    }
  return (ret);
}

#ifdef _NANO_FORMATTED_IO
int
_EXFUN(asiprintf, (char **, const char *, ...)
       _ATTRIBUTE ((__alias__("asprintf"))));
#endif
#endif /* ! _REENT_ONLY */
@



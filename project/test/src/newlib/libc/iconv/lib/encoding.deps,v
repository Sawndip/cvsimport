ALIASES: euctw
ENTRY END

SECTION END

#
# This section is named "CES_DEPENDENCIES" and describes dependencies
# between CES converters (some CES converters may use another CES converters).
#
SECTION CES_DEPENDENCIES
 
ENTRY
CES: table_pcs
USED_CES: table
ENTRY END

ENTRY
CES: euc
USED_CES: table us_ascii
ENTRY END

SECTION END


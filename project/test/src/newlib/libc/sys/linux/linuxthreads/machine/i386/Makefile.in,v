	@@for dep in $?; do \
	  case '$(am__configure_deps)' in \
	    *$$dep*) \
	      echo ' cd $(srcdir) && $(AUTOMAKE) --cygnus '; \
	      cd $(srcdir) && $(AUTOMAKE) --cygnus  \
		&& exit 0; \
	      exit 1;; \
	  esac; \

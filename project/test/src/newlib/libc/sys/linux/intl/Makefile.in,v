	$(CC) $(DEFS) $(DEFAULT_INCLUDES) $(INCLUDES) $(AM_CPPFLAGS) $(CPPFLAGS) $(lib_a_CFLAGS) $(CFLAGS) -c -o lib_a-bindtextdom.obj `if test -f 'bindtextdom.c'; then $(CYGPATH_W) 'bindtextdom.c'; else $(CYGPATH_W) '$(srcdir)/bindtextdom.c'; fi`


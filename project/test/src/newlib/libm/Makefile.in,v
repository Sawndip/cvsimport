.texinfo.html:
	rm -rf $(@@:.html=.htp)
	if $(MAKEINFOHTML) $(AM_MAKEINFOHTMLFLAGS) $(MAKEINFOFLAGS) -I $(srcdir) \
	 -o $(@@:.html=.htp) $<; \
	then \
	  rm -rf $@@; \
	  if test ! -d $(@@:.html=.htp) && test -d $(@@:.html=); then \
	    mv $(@@:.html=) $@@; else mv $(@@:.html=.htp) $@@; fi; \
	else \
	  if test ! -d $(@@:.html=.htp) && test -d $(@@:.html=); then \
	    rm -rf $(@@:.html=); else rm -Rf $(@@:.html=.htp) $@@; fi; \
	  exit 1; \
	fi

    m68k-*-rtemscoff*
    m68k-*-sunos*
    m68k-*-sysv*
    m68k-*-vxworks*
    m68k-apollo-*
    m68k-apple-aux*
    m68k-bull-sysv3*
    m68k-ericsson-ose
    m68k-motorola-sysv*
    m88k-*-*					(*)
    mn10200-*-*
    openrisc-*-*
    or32-*-*
    or32-*-coff
    pj*-*-*

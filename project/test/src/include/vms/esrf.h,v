#ifndef _VMS_ESRF_H
#define _VMS_ESRF_H

struct vms_esrf
{
  struct vms_egsy header;

  unsigned char namlng;
  unsigned char name[31];
};

#define ESRF__B_NAMLNG   8

#endif /* _VMS_ESRF_H */

#define IMAGE_FILE_NET_RUN_FROM_SWAP         0x0800
d91 18
a108 31
#define IMAGE_FILE_MACHINE_UNKNOWN           0x0000
#define IMAGE_FILE_MACHINE_ALPHA             0x0184

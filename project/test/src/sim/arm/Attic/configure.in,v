1.4
log
@Add iWMMXt support to ARM simulator
@
text
@d10 1
a10 8
COPRO="armcopro.o maverick.o"
CON_FLAGS=  
case x$target_alias in
	xxscale-*)
		COPRO="armcopro.o maverick.o iwmmxt.o"
		CON_FLAGS=-D__IWMMXT__
		;;
esac
a11 1
AC_SUBST(CON_FLAGS)
@



  withval=$with_pkgversion; case "$withval" in
      yes) as_fn_error "package version not specified" "$LINENO" 5 ;;
      no)  PKGVERSION= ;;
      *)   PKGVERSION="($withval) " ;;
     esac
else
  PKGVERSION="(GDB) "

fi






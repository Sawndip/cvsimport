    case " $sim_hw " in
      *" $i "*) ;;
      *) sim_hw="$sim_hw $i" ; sim_hw_objs="$sim_hw_objs dv-$i.o";;
    esac
  done
  # mingw does not support sockser
  SIM_DV_SOCKSER_O=""
  case ${host} in
    *mingw*) ;;
    *) SIM_DV_SOCKSER_O="dv-sockser.o"


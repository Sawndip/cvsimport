1.1
log
@	* sim/cris/hw: New directory for subdirectories with tests.
	* sim/cris/hw/rv-n-cris: New directory with tests.
@
text
@#mach: crisv32
#sim(crisv32): --hw-device "/rv/dummy /dev/zero"

# Test dummy settings: set from file.

 .include "testutils.inc"
 start
 move.d 0xd0000000,$r0
 move.d [$r0+],$r3
 beq ok
 nop
bad:
 fail
ok:
 moveq -1,$r3
 move.d $r3,[$r0]
 cmp.d [$r0],$r3
 bne bad
 nop
 pass
@

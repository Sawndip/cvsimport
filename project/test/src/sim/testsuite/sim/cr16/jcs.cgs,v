log
@

  New files: Testcases for cr16 instruction set.
@
text
@# cr16 testcase for jcs (regp)
# mach(): cr16

	.include "testutils.inc"

	start

	.global jcs
jcs:
        movd $ok, (r7,r6)
	lshd $-1, (r7,r6)

	mvi_h_condbit 0
	movw $12, r4
	movw $10, r5
	subw r4, r5
	jcs (r7,r6)
not_ok:
	fail
ok:
	pass
@

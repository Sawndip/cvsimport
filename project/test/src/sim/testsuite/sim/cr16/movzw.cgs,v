text
@# cr16 testcase for movzw  reg, regp
# mach(): cr16

	.include "testutils.inc"

	start

	.global movzw
movzw:
	movb $0xff, r4
	movd $0x12345678,(r6, r5)

	movzw r4, (r6,r5)

	test_h_grp "(r6, r5)", 0xff

	pass
@



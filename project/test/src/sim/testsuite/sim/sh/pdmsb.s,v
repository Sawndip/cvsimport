	set_sreg 0xfffff, x0
L20:	pdmsb	x0, x1
	assert_sreg 11<<16, x1
	set_sreg 0x1fffff, x0
L21:	pdmsb	x0, x1
	assert_sreg 10<<16, x1
	set_sreg 0x3fffff, x0
L22:	pdmsb	x0, x1
	assert_sreg 9<<16, x1
	set_sreg 0x7fffff, x0
L23:	pdmsb	x0, x1
	assert_sreg 8<<16, x1
	set_sreg 0xffffff, x0
L24:	pdmsb	x0, x1
	assert_sreg 7<<16, x1

	set_sreg 0x1ffffff, x0
L25:	pdmsb	x0, x1
	assert_sreg 6<<16, x1
	set_sreg 0x3ffffff, x0
L26:	pdmsb	x0, x1
	assert_sreg 5<<16, x1
	set_sreg 0x7ffffff, x0

	;; mov.b #xx:8, @@(dd:16, erd.l)
	mov.b	#0xa5:8, @@(byte_dst-3:16, er1.l) ; byte indexed operand

	;; test ccr		; H=0 N=1 Z=0 V=0 C=0
	test_neg_set
	test_zero_clear
	test_ovf_clear
	test_carry_clear

	test_gr_a5a5 0		; Make sure other general regs not disturbed

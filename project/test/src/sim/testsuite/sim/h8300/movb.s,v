	set_ccr_zero

	;; mov.b @@(dd:16, ers), rd
	mov.l	#byte_src+0x1234, er1
	mov.b	@@(-0x1234:16, er1), r0l	; Register plus 16-bit disp. operand
;;;	.word	0x6e18
;;;	.word	-0x1234

	;; test ccr		; H=0 N=0 Z=0 V=0 C=0
	test_neg_clear
	test_zero_clear
	test_ovf_clear
	test_carry_clear


	set_ccr_zero
	;; mov.b #xx:8, @@(dd:32, rd.w)
	mov.b	#0xa5:8, @@(byte_dst-5:32, r1.w)	; byte indexed operand

	;; test ccr		; H=0 N=1 Z=0 V=0 C=0

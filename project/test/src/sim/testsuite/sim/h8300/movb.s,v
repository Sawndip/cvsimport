	set_ccr_zero
	;; mov.b #xx:8, @@(dd:32, rd.b)
	mov.b	#0xa5:8, @@(byte_dst-4:32, r1.b)	; byte indexed operand

	;; test ccr		; H=0 N=1 Z=0 V=0 C=0
	test_neg_set
	test_zero_clear
	test_ovf_clear
	test_carry_clear


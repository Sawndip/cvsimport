	test_grs_a5a5		; general registers should not be changed.

.if (sim_cpu > h8300h)
bild_imm3_abs16:
	set_grs_a5a5
	set_ccr_zero
	;; bild xx:3, aa:16
	bild	#7, @@byte_src:16	; this should NOT set the carry flag.

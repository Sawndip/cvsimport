	test_gr_a5a5 7

bild_imm3_abs8:
	set_grs_a5a5
	mov.b	r1l, @@0x20
	set_ccr_zero
	;; bild xx:3, aa:8
	bild	#7, @@0x20:8	; this should NOT set the carry flag.
	test_cc_clear
	bild	#6, @@0x20:8	; this should set the carry flag.
	test_carry_set
	test_ovf_clear

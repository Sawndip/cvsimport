if test "${enable_sim_warnings+set}" = set; then
  enableval="$enable_sim_warnings"
  case "${enableval}" in
  yes)	sim_warnings="-Werror -Wall -Wpointer-arith -Wmissing-prototypes -Wmissing-declarations ";;
  no)	sim_warnings="-w";;

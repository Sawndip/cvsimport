/* end confdefs.h.  */
#include <sys/types.h>
#include <sys/termios.h>
int
main ()
{
static struct termios x;
 x.c_iflag = 0;
 x.c_oflag = 0;
 x.c_cflag = 0;
 x.c_lflag = 0;
 x.c_cc[NCCS] = 0;
  ;
  return 0;
}
_ACEOF

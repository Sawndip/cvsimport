{
      -3,     1,     7,    27,     9,    10,    11,    34,    35,    28,
      20,     2,     3,    14,    36,     9,    10,    11,    17,    22,
      12,     9,    10,    11,    34,    35,    46,    33,    23,    26,
      47,    41,    24,    31,    32,    50,    44,    42,    51,     8,
      43,    48,    13,    45,    16,    19

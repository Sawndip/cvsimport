       0,     0,     2,     5,     6,    12,    13,    23,    25,    28,
      32,    35,    36,    38,    40,    42,    46,    50,    54,    56,
      59,    60,    65,    66,    68,    70,    72,    74,    76,    78
};
static const short yyrhs[] =

	    (yyval.val) = (yyvsp[(1) - (1)].num);
	  }
    break;

  case 20:
#line 377 "itbl-parse.y"
    {
	    DBGL2 (("flags=%d\n", (yyvsp[(2) - (2)].val)));
	    (yyval.val) = (yyvsp[(2) - (2)].val);
	  }
    break;


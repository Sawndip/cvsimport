      21,     0,    22,    21,     0,     0,    30,    31,    32,    33,
      11,     0,     0,    30,     8,    32,    33,    29,    28,    23,
      24,    11,     0,    11,     0,     1,    11,     0,    13,    26,
      24,     0,    26,    24,     0,     0,    31,     0,     7,     0,
       6,     0,    25,    29,    28,     0,     9,    14,    27,     0,

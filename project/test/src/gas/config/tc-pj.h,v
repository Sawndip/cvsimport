/* Always leave vtable relocs untouched in the output.  */
#define TC_FORCE_RELOCATION(FIX)                                  	\
          ((FIX)->fx_r_type == BFD_RELOC_VTABLE_INHERIT           	\
	   || (FIX)->fx_r_type == BFD_RELOC_VTABLE_ENTRY		\
	   || S_FORCE_RELOC ((FIX)->fx_addsy))
@



    jsr @@@@0x234          ;5d8d

    rts                      ;5470     
    rts/l  er3               ;5403
    rts/l  (er1-er2)         ;5412
    rts/l  (er2-er4)         ;5424
    rts/l  (er3-er6)         ;5436

    trapa #0x3               ;5730 

    rte                      ;5670     
    rte/l  er3               ;5603
    rte/l  (er1-er2)         ;5612
    rte/l  (er2-er4)         ;5624
    rte/l  (er3-er6)         ;5636

    ldc.b #0x12:8,ccr        ;0712 
    ldc.b r3h,ccr            ;0303 
    ldc.w @@er3,ccr           ;01406930 
    ldc.w @@er3+,ccr          ;01406d30 

    stc.b ccr,r1h            ;0201 
    stc.w ccr,@@er1           ;01406990 
    stc.w ccr,@@-er1          ;01406d90 
    stc.w ccr,@@(0x1234:16,er1)       ;01406f901234
    stc.w ccr,@@(0x12345678:32,er1)   ;014078106ba012345678
    stc.w ccr,@@0x1234:16             ;01406b801234

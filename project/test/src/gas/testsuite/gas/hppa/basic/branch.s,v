	addb,=,n %r1,%r4,addb_tests
	addb,<,n %r1,%r4,addb_tests
	addb,<=,n %r1,%r4,addb_tests
	addb,nuv,n %r1,%r4,addb_tests
	addb,znv,n %r1,%r4,addb_tests
	addb,sv,n %r1,%r4,addb_tests
	addb,od,n %r1,%r4,addb_tests

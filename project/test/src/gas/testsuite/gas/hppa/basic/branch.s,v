	addb,<>,n %r1,%r4,addb_tests
	addb,>=,n %r1,%r4,addb_tests
	addb,>,n %r1,%r4,addb_tests
	addb,uv,n %r1,%r4,addb_tests
	addb,vnz,n %r1,%r4,addb_tests
	addb,nsv,n %r1,%r4,addb_tests
	addb,ev,n %r1,%r4,addb_tests

addib_tests: 
	addib -1,%r4,addib_tests
	addib,= -1,%r4,addib_tests
	addib,< -1,%r4,addib_tests
	addib,<= -1,%r4,addib_tests
	addib,nuv -1,%r4,addib_tests

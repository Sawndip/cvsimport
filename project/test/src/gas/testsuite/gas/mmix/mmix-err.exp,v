if { ! [istarget "mmix-*"] } {
    return
}

proc run_mmix_err_tests { } {
    global srcdir subdir runtests

    load_lib gas-dg.exp
    dg-init
    dg-runtest [lsort [glob -nocomplain $srcdir/$subdir/err-*.s]] "" ""
    dg-finish
}

run_mmix_err_tests

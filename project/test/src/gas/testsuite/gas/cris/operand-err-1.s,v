1.1
log
@	* gas/cris/*: New tests for CRIS.
@
text
@d32 2
a33 2
 test.d [r3=r2+] ; { dg-error "(Illegal|Invalid) operands|Bad expression" }
 test.d [r3++] ; { dg-error "(Illegal|Invalid) operands|Bad expression" }
@


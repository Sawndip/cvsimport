@; Tests the broken-word function with a real switch table.  CRISv32 version.

start:	moveq 0,r0

	subs.b 87,r0
	bound.b 41,r0
	lapc sym2,acr
	addi r0.w,acr
	adds.w [acr],acr
	jump acr
	nop
sym2:
	.word	sym1 - .
	.word	sym3 - .

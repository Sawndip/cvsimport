	.word	sym39 - sym2
	.word	sym40 - sym2
	.word	sym41 - sym2
	.word	sym42 - sym2
	.word	sym43 - sym2

	.space	16, 0

	moveq 1,r0
; Medium-range branch around secondary jump table inserted here :
;	ba	next_label
;	nop
;	.skip	2,0
; Secondary jump table inserted here :
;	jump	sym1
;	jump	sym3
;	...
next_label:
	moveq 2,r0

	.space	32768, 0

sym1:	moveq -3,r0
sym3: moveq 3,r0
sym4: moveq 4,r0
sym5: moveq 5,r0

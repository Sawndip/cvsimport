1.2
log
@	* gas/cris: Match output having $ register prefix.  Deal with
	target variants default requiring register prefix on input.
@
text
@; Tests the broken-word function, one more word than fits in a
; single branch.
	.syntax no_register_prefix
start:	moveq 0,r0

; Take the opportunity to (rudimentally) test case-recognition,
; as the diassembler gets overly confused by the table.
	subs.b 87,r0
	bound.b 41,r0
	adds.w [pc+r0.w],pc 
sym2:
	.word	sym1 - sym2
	.word	sym3 - sym2
	.word	sym4 - sym2
	.word	sym5 - sym2
	.word	sym6 - sym2
	.word	sym7 - sym2

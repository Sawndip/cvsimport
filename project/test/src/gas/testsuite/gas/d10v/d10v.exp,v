1.2
log
@Some d10v gas tests.
@
text
@a3 13
proc run_list_test { name opts } {
  global srcdir subdir
  set testname "D10V $name"
  set file $srcdir/$subdir/$name
  gas_run ${name}.s $opts ">&dump.out"
  if {[regexp_diff "dump.out" "${file}.l"] } {
    fail $testname
    verbose "output is [file_contents "dump.out"]" 2
    return
  }
  pass $testname
}


text
@# Miscellaneous instructions.
 .data
foodata: .word 42
	 .text
footext:

# Multiply instructions.
	.global macsb
macsb:
macsb r0 , r1

	.global macub
macub:
macub r2 , r3

	.global macqb
macqb:
macqb r4 , r5

	.global macsw
macsw:

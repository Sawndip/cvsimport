   see <http://www.gnu.org/licenses/>.  */

	.macro asimdabs	op, T
	\op	v0.\()\T, v31.\()\T
	.endm

	.text
	.irp	op, abs, neg, sqabs, sqneg
	.irp	type, 8b, 16b, 4h, 8h, 2s, 4s, 2d
	asimdabs	\op \type
	.endr
	.endr
@



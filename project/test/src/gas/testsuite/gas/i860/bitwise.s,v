	xor	%r21,%r22,%r23
	xor	%r24,%r25,%r26
	xor	%r27,%r28,%r29
	xor	%r30,%r31,%r0

	# Immediate forms (all)
	and	0,%r1,%r2
	and	8192,%r4,%r5
	and	5109,%r7,%r8
	and	32768,%r10,%r11
	and	65000,%r13,%r14

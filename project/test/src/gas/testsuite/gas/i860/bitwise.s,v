@# and, andh, andnot, andnoth, or, orh, xor, xorh 

	.text

	# Register forms (high variants do not have register forms).
	and	%r0,%r1,%r2
	and	%r3,%r4,%r5
	and	%r6,%r7,%r8
	and	%r9,%r10,%r11
	and	%r12,%r13,%r14
	and	%r15,%r16,%r17
	and	%r18,%r19,%r20
	and	%r21,%r22,%r23
	and	%r24,%r25,%r26
	and	%r27,%r28,%r29
	and	%r30,%r31,%r0

	andnot	%r0,%r1,%r2
	andnot	%r3,%r4,%r5
	andnot	%r6,%r7,%r8
	andnot	%r9,%r10,%r11

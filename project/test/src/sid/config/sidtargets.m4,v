text
@a47 1
sidtarget_sh_or_sh64=$sidtarget_default
d63 2
a64 4
      sh64*) sidtarget_sh64=1
	     sidtarget_sh_or_sh64=1 ;;
      sh*)   sidtarget_sh=1
	     sidtarget_sh_or_sh64=1 ;;
a134 5

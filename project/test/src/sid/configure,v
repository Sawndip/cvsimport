$as_echo_n "checking MT family support... " >&6; }

 if test "x$sidtarget_mt" = x1; then
  SIDTARGET_MT_TRUE=
  SIDTARGET_MT_FALSE='#'
else
  SIDTARGET_MT_TRUE='#'
  SIDTARGET_MT_FALSE=
fi

{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $sidtarget_mt" >&5
$as_echo "$sidtarget_mt" >&6; }

{ $as_echo "$as_me:${as_lineno-$LINENO}: checking PPC family support" >&5
$as_echo_n "checking PPC family support... " >&6; }

 if test "x$sidtarget_ppc" = x1; then
  SIDTARGET_PPC_TRUE=
  SIDTARGET_PPC_FALSE='#'
else
  SIDTARGET_PPC_TRUE='#'
  SIDTARGET_PPC_FALSE=
fi

{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $sidtarget_ppc" >&5
$as_echo "$sidtarget_ppc" >&6; }

{ $as_echo "$as_me:${as_lineno-$LINENO}: checking SH family support" >&5
$as_echo_n "checking SH family support... " >&6; }

 if test "x$sidtarget_sh" = x1; then
  SIDTARGET_SH_TRUE=

a2202 24

# Check whether --enable-timebomb or --disable-timebomb was given.
if test "${enable_timebomb+set}" = set; then
  enableval="$enable_timebomb"
  :
fi
	
SID_TIMEBOMB=0
if test -z "$enable_timebomb"
then
	SID_TIMEBOMB=0
elif test "$enable_timebomb" = "no"
then
	SID_TIMEBOMB=0
elif expr "$enable_timebomb" : '[0-9]*$' >/dev/null
then
	SID_TIMEBOMB=$enable_timebomb
# XXX: support relative "days from now" format
else
	{ echo "configure: error: Must specify timebomb time in proper format" 1>&2; exit 1; }
fi
cat >> confdefs.h <<EOF
#define SID_TIMEBOMB $SID_TIMEBOMB
EOF
@


  as_fn_error "conditional \"HAVE_XSLTPROC\" was never defined.
Usually this means the macro was only invoked conditionally." "$LINENO" 5
fi
if test -z "${HAVE_SABLOTRON_TRUE}" && test -z "${HAVE_SABLOTRON_FALSE}"; then
  as_fn_error "conditional \"HAVE_SABLOTRON\" was never defined.
Usually this means the macro was only invoked conditionally." "$LINENO" 5
fi
if test -z "${SIDTARGET_ARM_TRUE}" && test -z "${SIDTARGET_ARM_FALSE}"; then
  as_fn_error "conditional \"SIDTARGET_ARM\" was never defined.
Usually this means the macro was only invoked conditionally." "$LINENO" 5
fi
if test -z "${SIDTARGET_X86_TRUE}" && test -z "${SIDTARGET_X86_FALSE}"; then
  as_fn_error "conditional \"SIDTARGET_X86\" was never defined.
Usually this means the macro was only invoked conditionally." "$LINENO" 5
fi
if test -z "${SIDTARGET_MIPS_TRUE}" && test -z "${SIDTARGET_MIPS_FALSE}"; then
  as_fn_error "conditional \"SIDTARGET_MIPS\" was never defined.
Usually this means the macro was only invoked conditionally." "$LINENO" 5
fi
if test -z "${SIDTARGET_M32R_TRUE}" && test -z "${SIDTARGET_M32R_FALSE}"; then
  as_fn_error "conditional \"SIDTARGET_M32R\" was never defined.
Usually this means the macro was only invoked conditionally." "$LINENO" 5
fi
if test -z "${SIDTARGET_M68K_TRUE}" && test -z "${SIDTARGET_M68K_FALSE}"; then
  as_fn_error "conditional \"SIDTARGET_M68K\" was never defined.
Usually this means the macro was only invoked conditionally." "$LINENO" 5
fi

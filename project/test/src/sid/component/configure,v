{ $as_echo "$as_me:${as_lineno-$LINENO}: checking whether ftime is declared" >&5
$as_echo_n "checking whether ftime is declared... " >&6; }
if test "${ac_cv_decl_ftime+set}" = set; then :
  $as_echo_n "(cached) " >&6
else

cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */
#include <sys/timeb.h>

_ACEOF
if (eval "$ac_cpp conftest.$ac_ext") 2>&5 |
  $EGREP "ftime" >/dev/null 2>&1; then :
  ac_cv_decl_ftime=yes
else
  ac_cv_decl_ftime=no

fi
rm -f conftest*

fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_cv_decl_ftime" >&5
$as_echo "$ac_cv_decl_ftime" >&6; }
if test x$ac_cv_decl_ftime = xyes; then

$as_echo "#define HAVE_FTIME_DECL 1" >>confdefs.h

fi

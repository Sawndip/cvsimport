current_cpu->check_write_to_text (* FLD (i_rma));
current_cpu->do_smcp (* FLD (i_rma), current_cpu->hardware.h_cr64[FLD (f_crn)], pc);
}

  current_cpu->done_insn (npc, status);
  return status;
#undef FLD
}


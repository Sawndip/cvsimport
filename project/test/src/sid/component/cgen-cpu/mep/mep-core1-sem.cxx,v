  PCADDR npc = pc + 2;

{
current_cpu->check_option_dcache (pc);
current_cpu->do_cache_prefetch (FLD (f_rn), * FLD (i_rma), pc);
}

  current_cpu->done_insn (npc, status);
  return status;
#undef FLD
}


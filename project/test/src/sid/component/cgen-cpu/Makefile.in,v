check-local: all
	-(cd ../testsuite ; $(MAKE) check RUNTESTFLAGS="$(RUNTESTFLAGS) $(DEJAGNUTESTS)" )
	for subdir in @@cpu_subdirs@@; do \
		(cd $$subdir && $(MAKE) $(AM_MAKEFLAGS) check); \
	done

.PHONY: cgen-all
cgen-all:
	for subdir in @@cpu_subdirs@@; do \
		(cd $$subdir && $(MAKE) $(AM_MAKEFLAGS) cgen-all) || exit 1 ; \
	done


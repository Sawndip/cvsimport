// ********** syscall: --unused--

sem_status
xstormy16_sem_syscall (xstormy16_cpu* current_cpu, xstormy16_scache* sem)
{
#define FLD(f) abuf->fields.sfmt_empty.f
  sem_status status = SEM_STATUS_NORMAL;
  xstormy16_scache* abuf = sem;
  PCADDR pc = abuf->addr;
  PCADDR npc = pc + 2;

current_cpu->syscall ();

  current_cpu->done_insn (npc, status);
  return status;
#undef FLD
}


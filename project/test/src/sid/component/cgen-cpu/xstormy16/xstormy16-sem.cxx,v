  xstormy16_scache* abuf = sem;
  PCADDR pc = abuf->addr;
  PCADDR npc = pc + 2;

((void) 0); /*nop*/

  current_cpu->done_insn (npc, status);
  return status;
#undef FLD
}

// ********** halt: halt

sem_status
xstormy16_sem_halt (xstormy16_cpu* current_cpu, xstormy16_scache* sem)
{
#define FLD(f) abuf->fields.sfmt_empty.f
  sem_status status = SEM_STATUS_NORMAL;
  xstormy16_scache* abuf = sem;
  PCADDR pc = abuf->addr;
  PCADDR npc = pc + 2;

current_cpu->do_halt ();

  current_cpu->done_insn (npc, status);
  return status;
#undef FLD
}


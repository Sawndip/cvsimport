    UINT f_Rd;
    UINT f_imm4;
  } sfmt_bngrimm4;
  struct { /*  */
    SI f_rel8_4;
    UINT f_imm16;
    UINT f_op2;
  } sfmt_bccimm16;
  struct { /*  */
    UINT f_Rm;
    UINT f_op2m;
    HI f_hmem8;
  } sfmt_movhgrmem;
  struct { /*  */
    UINT f_Rm;
    UINT f_lmem8;
    UINT f_op2m;
  } sfmt_movlgrmem;
  struct { /*  */
    UINT f_imm16;

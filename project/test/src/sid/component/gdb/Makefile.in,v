@@am__fastdepCC_TRUE@@	$(LTCOMPILE) -MT $@@ -MD -MP -MF $(DEPDIR)/$*.Tpo -c -o $@@ $<
@@am__fastdepCC_TRUE@@	$(am__mv) $(DEPDIR)/$*.Tpo $(DEPDIR)/$*.Plo
@@AMDEP_TRUE@@@@am__fastdepCC_FALSE@@	source='$<' object='$@@' libtool=yes @@AMDEPBACKSLASH@@

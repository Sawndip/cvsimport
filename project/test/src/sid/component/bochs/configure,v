$as_echo_n "checking for CDROM support... " >&6; }
# Check whether --enable-cdrom was given.
if test "${enable_cdrom+set}" = set; then :
  enableval=$enable_cdrom; if test "$enableval" = yes; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
    CDROM_OBJS='cdrom.o'
    $as_echo "#define BX_SUPPORT_CDROM 1" >>confdefs.h

   elif test "$enableval" = no; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    CDROM_OBJS=''
    $as_echo "#define BX_SUPPORT_CDROM 0" >>confdefs.h

   else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
    CDROM_OBJS='cdrom.o'
    $as_echo "#define BX_SUPPORT_CDROM 1" >>confdefs.h

   fi
else

    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    CDROM_OBJS=''
    $as_echo "#define BX_SUPPORT_CDROM 0" >>confdefs.h



fi




{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for Sound Blaster 16 support" >&5
$as_echo_n "checking for Sound Blaster 16 support... " >&6; }
# Check whether --enable-sb16 was given.
if test "${enable_sb16+set}" = set; then :
  enableval=$enable_sb16; if test "$enableval" = no; then
     { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
     SB16_OBJS=''
     $as_echo "#define BX_SUPPORT_SB16 0" >>confdefs.h

   else
     case "$enableval" in
       dummy)
         SB16_OBJS='$(SB16_DUMMY_OBJS)'
         $as_echo "#define BX_SOUND_OUTPUT_C bx_sound_output_c" >>confdefs.h

         { $as_echo "$as_me:${as_lineno-$LINENO}: result: dummy" >&5
$as_echo "dummy" >&6; }
         ;;
       linux)
         SB16_OBJS='$(SB16_LINUX_OBJS)'
         $as_echo "#define BX_SOUND_OUTPUT_C bx_sound_linux_c" >>confdefs.h

         { $as_echo "$as_me:${as_lineno-$LINENO}: result: linux" >&5
$as_echo "linux" >&6; }
         ;;
       win)
         SB16_OBJS='$(SB16_WIN_OBJS)'
         $as_echo "#define BX_SOUND_OUTPUT_C bx_sound_windows_c" >>confdefs.h

         { $as_echo "$as_me:${as_lineno-$LINENO}: result: win" >&5
$as_echo "win" >&6; }
         ;;
       *)
         echo " "
         echo \!\!\!Error\!\!\!
         echo "You must pass one of dummy, linux, win to --enable-sb16"
         exit 1 ;;
     esac
     $as_echo "#define BX_SUPPORT_SB16 1" >>confdefs.h

   fi
else

    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    SB16_OBJS=''
    $as_echo "#define BX_SUPPORT_SB16 0" >>confdefs.h



fi




# Check whether --enable-hga-dumps was given.
if test "${enable_hga_dumps+set}" = set; then :
  enableval=$enable_hga_dumps; cat >>confdefs.h <<_ACEOF
#define BX_EMULATE_HGA_DUMPS $enableval
_ACEOF

fi



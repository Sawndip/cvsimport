  ac_fn_c_check_func "$LINENO" "strtoull" "ac_cv_func_strtoull"
if test "x$ac_cv_func_strtoull" = x""yes; then :
  cat >>confdefs.h <<_ACEOF
#define HAVE_STRTOULL 1
_ACEOF
 $as_echo "#define BX_HAVE_STRTOULL 1" >>confdefs.h

fi
done

for ac_func in strtouq
do :
  ac_fn_c_check_func "$LINENO" "strtouq" "ac_cv_func_strtouq"
if test "x$ac_cv_func_strtouq" = x""yes; then :
  cat >>confdefs.h <<_ACEOF
#define HAVE_STRTOUQ 1
_ACEOF
 $as_echo "#define BX_HAVE_STRTOUQ 1" >>confdefs.h

fi
done

for ac_func in strdup
do :
  ac_fn_c_check_func "$LINENO" "strdup" "ac_cv_func_strdup"
if test "x$ac_cv_func_strdup" = x""yes; then :
  cat >>confdefs.h <<_ACEOF
#define HAVE_STRDUP 1
_ACEOF
 $as_echo "#define BX_HAVE_STRDUP 1" >>confdefs.h

fi
done


{ $as_echo "$as_me:${as_lineno-$LINENO}: checking if compiler allows empty structs" >&5
$as_echo_n "checking if compiler allows empty structs... " >&6; }
cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

int
main ()
{
typedef struct { } junk;
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_compile "$LINENO"; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
else

      $as_echo "#define BX_NO_EMPTY_STRUCTS 1" >>confdefs.h

      { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }

fi
rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext

{ $as_echo "$as_me:${as_lineno-$LINENO}: checking if compiler allows __attribute__" >&5
$as_echo_n "checking if compiler allows __attribute__... " >&6; }
cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

int
main ()
{
typedef struct { } __attribute__ ((packed)) junk;
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_compile "$LINENO"; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
else

      { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
      $as_echo "#define BX_NO_ATTRIBUTES 1" >>confdefs.h


fi
rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext


ac_ext=cpp
ac_cpp='$CXXCPP $CPPFLAGS'
ac_compile='$CXX -c $CXXFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CXX -o conftest$ac_exeext $CXXFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_cxx_compiler_gnu

{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for hash_map.h" >&5
$as_echo_n "checking for hash_map.h... " >&6; }
cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */
#include <hash_map.h>
int
main ()
{

  ;
  return 0;
}
_ACEOF
if ac_fn_cxx_try_compile "$LINENO"; then :

     { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
     $as_echo "#define BX_HAVE_HASH_MAP 1" >>confdefs.h


else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi
rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu


{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for number of processors" >&5
$as_echo_n "checking for number of processors... " >&6; }
# Check whether --enable-processors was given.
if test "${enable_processors+set}" = set; then :
  enableval=$enable_processors; case "$enableval" in
     1)
       { $as_echo "$as_me:${as_lineno-$LINENO}: result: 1" >&5
$as_echo "1" >&6; }
       $as_echo "#define BX_SMP_PROCESSORS 1" >>confdefs.h

       $as_echo "#define BX_BOOTSTRAP_PROCESSOR 0" >>confdefs.h

       $as_echo "#define BX_IOAPIC_DEFAULT_ID 1" >>confdefs.h

       ;;
     2)
       { $as_echo "$as_me:${as_lineno-$LINENO}: result: 2" >&5
$as_echo "2" >&6; }
       $as_echo "#define BX_SMP_PROCESSORS 2" >>confdefs.h

       $as_echo "#define BX_BOOTSTRAP_PROCESSOR 0" >>confdefs.h

       $as_echo "#define BX_IOAPIC_DEFAULT_ID 2" >>confdefs.h

       $as_echo "#define BX_USE_CPU_SMF 0" >>confdefs.h

       ;;
     4)
       { $as_echo "$as_me:${as_lineno-$LINENO}: result: 4" >&5
$as_echo "4" >&6; }
       $as_echo "#define BX_SMP_PROCESSORS 4" >>confdefs.h

       $as_echo "#define BX_BOOTSTRAP_PROCESSOR 2" >>confdefs.h

       $as_echo "#define BX_IOAPIC_DEFAULT_ID 4" >>confdefs.h

       $as_echo "#define BX_USE_CPU_SMF 0" >>confdefs.h

       ;;
     *)
       echo " "
       echo "WARNING: processors != 1,2,4 can work, but you need to modify rombios.c manually"
       { $as_echo "$as_me:${as_lineno-$LINENO}: result: $enable_val" >&5
$as_echo "$enable_val" >&6; }
       $as_echo "#define BX_SMP_PROCESSORS \$enable_val" >>confdefs.h

       $as_echo "#define BX_BOOTSTRAP_PROCESSOR 0" >>confdefs.h

       $as_echo "#define BX_IOAPIC_DEFAULT_ID \$enable_val" >>confdefs.h

       $as_echo "#define BX_USE_CPU_SMF 0" >>confdefs.h

       ;;
   esac
   bx_procs="$enableval"

else

     { $as_echo "$as_me:${as_lineno-$LINENO}: result: 1" >&5
$as_echo "1" >&6; }
     $as_echo "#define BX_SMP_PROCESSORS 1" >>confdefs.h

     $as_echo "#define BX_BOOTSTRAP_PROCESSOR 0" >>confdefs.h

     $as_echo "#define BX_IOAPIC_DEFAULT_ID 1" >>confdefs.h

     bx_procs=1


fi


{ $as_echo "$as_me:${as_lineno-$LINENO}: checking if compiler allows blank labels" >&5
$as_echo_n "checking if compiler allows blank labels... " >&6; }
cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

int
main ()
{
 { label1: }
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_compile "$LINENO"; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
else

      { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
      $as_echo "#define BX_NO_BLANK_LABELS 1" >>confdefs.h


fi
rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext

{ $as_echo "$as_me:${as_lineno-$LINENO}: checking if compiler allows LL for 64-bit constants" >&5
$as_echo_n "checking if compiler allows LL for 64-bit constants... " >&6; }
cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

int
main ()
{
 { 42LL; }
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_compile "$LINENO"; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
else

      { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
      $as_echo "#define BX_64BIT_CONSTANTS_USE_LL 0" >>confdefs.h


fi
rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext

{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for cpu level" >&5
$as_echo_n "checking for cpu level... " >&6; }
# Check whether --enable-cpu-level was given.
if test "${enable_cpu_level+set}" = set; then :
  enableval=$enable_cpu_level; case "$enableval" in
     3)
       { $as_echo "$as_me:${as_lineno-$LINENO}: result: 3" >&5
$as_echo "3" >&6; }
       $as_echo "#define BX_CPU_LEVEL 3" >>confdefs.h

       $as_echo "#define BX_CPU_LEVEL_HACKED 3" >>confdefs.h

       ;;
     4)
       { $as_echo "$as_me:${as_lineno-$LINENO}: result: 4" >&5
$as_echo "4" >&6; }
       $as_echo "#define BX_CPU_LEVEL 4" >>confdefs.h

       $as_echo "#define BX_CPU_LEVEL_HACKED 4" >>confdefs.h

       ;;
     5)
       { $as_echo "$as_me:${as_lineno-$LINENO}: result: 5" >&5
$as_echo "5" >&6; }
       $as_echo "#define BX_CPU_LEVEL 5" >>confdefs.h

       $as_echo "#define BX_CPU_LEVEL_HACKED 5" >>confdefs.h

       ;;
     6)
       { $as_echo "$as_me:${as_lineno-$LINENO}: result: 6" >&5
$as_echo "6" >&6; }
       $as_echo "#define BX_CPU_LEVEL 6" >>confdefs.h

       $as_echo "#define BX_CPU_LEVEL_HACKED 6" >>confdefs.h

       ;;
     *)
       echo " "
       echo "ERROR: you must supply a valid CPU level to --enable-cpu-level"
       exit 1
       ;;
   esac
   bx_cpu_level=$enableval
   if test "$bx_procs" -gt 1 -a "$enableval" -lt 6; then
     echo "ERROR: with >1 processor, use --enable-cpu-level=6"
     exit 1
   fi

else

    # for multiprocessors, cpu level must be 6
    if test "$bx_procs" -gt 1; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: 6" >&5
$as_echo "6" >&6; }
      $as_echo "#define BX_CPU_LEVEL 6" >>confdefs.h

      $as_echo "#define BX_CPU_LEVEL_HACKED 6" >>confdefs.h

      bx_cpu_level=6
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: 5" >&5
$as_echo "5" >&6; }
      $as_echo "#define BX_CPU_LEVEL 5" >>confdefs.h

      $as_echo "#define BX_CPU_LEVEL_HACKED 5" >>confdefs.h

      bx_cpu_level=5
    fi


fi




{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for dynamic translation support" >&5
$as_echo_n "checking for dynamic translation support... " >&6; }
# Check whether --enable-dynamic was given.
if test "${enable_dynamic+set}" = set; then :
  enableval=$enable_dynamic; if test "$enableval" = no; then
     { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
     $as_echo "#define BX_DYNAMIC_TRANSLATION 0" >>confdefs.h

     DYNAMIC_VAR=''
     AS_DYNAMIC_OBJS=''
     AS_DYNAMIC_INCS=''
   else
     if test "$enableval" = yes; then
       echo " "
       echo \!\!\!Error\!\!\!
       echo "You must specify a CPU type to dynamic-translation option"
       echo \!\!\!Error\!\!\!
       exit 1
     fi
     case "$enableval" in
       i386)
         $as_echo "#define BX_DYNAMIC_CPU_I386 1" >>confdefs.h

         AS_DYNAMIC_OBJS='$(X86_OBJS)'
         AS_DYNAMIC_INCS='$(X86_H)'
         ;;
       sparc)
         $as_echo "#define BX_DYNAMIC_CPU_SPARC 1" >>confdefs.h

         AS_DYNAMIC_OBJS='$(SPARC_OBJS).o'
         AS_DYNAMIC_INCS='$(SPARC_H)'
         ;;
       *)
         echo " "
         echo \!\!\!Error\!\!\!
         echo "Sorry, dynamic translation is not yet available on your platform"
         echo \!\!\!Error\!\!\!
         AS_DYNAMIC_OBJS=''
         AS_DYNAMIC_INCS=''
         exit 1 ;;
     esac
     { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
     $as_echo "#define BX_DYNAMIC_TRANSLATION 1" >>confdefs.h

     DYNAMIC_VAR='$(DYNAMIC_LIB)'
   fi
else

    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    $as_echo "#define BX_DYNAMIC_TRANSLATION 0" >>confdefs.h

    DYNAMIC_VAR=''
    AS_DYNAMIC_OBJS=''
    AS_DYNAMIC_INCS=''


fi





{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for APIC support" >&5
$as_echo_n "checking for APIC support... " >&6; }
# Check whether --enable-apic was given.
if test "${enable_apic+set}" = set; then :
  enableval=$enable_apic; if test "$enableval" = yes; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
    $as_echo "#define BX_SUPPORT_APIC 1" >>confdefs.h

    IOAPIC_OBJS='ioapic.o'
    APIC_OBJS='apic.o'
   else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    if test "$bx_procs" -gt 1; then
      echo "Number of processors = $bx_procs"
      echo "ERROR: With processors > 1 you must use --enable-apic"
      exit 1
    fi
    $as_echo "#define BX_SUPPORT_APIC 0" >>confdefs.h

    IOAPIC_OBJS=''
    APIC_OBJS=''
   fi

else

    if test "$bx_procs" -gt 1 -o "$bx_cpu_level" -gt 5; then
      # enable APIC by default, if processors>1 or if cpulevel>5
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
      $as_echo "#define BX_SUPPORT_APIC 1" >>confdefs.h

      IOAPIC_OBJS='ioapic.o'
      APIC_OBJS='apic.o'
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
      $as_echo "#define BX_SUPPORT_APIC 0" >>confdefs.h

      IOAPIC_OBJS=''
      APIC_OBJS=''
     fi


fi




{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for split hard disk image support" >&5
$as_echo_n "checking for split hard disk image support... " >&6; }
# Check whether --enable-split-hd was given.
if test "${enable_split_hd+set}" = set; then :
  enableval=$enable_split_hd; if test "$enableval" = yes; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
    $as_echo "#define BX_SPLIT_HD_SUPPORT 1" >>confdefs.h

   else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    $as_echo "#define BX_SPLIT_HD_SUPPORT 0" >>confdefs.h

   fi
else

    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
    $as_echo "#define BX_SPLIT_HD_SUPPORT 1" >>confdefs.h



fi



{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for NE2000 support" >&5
$as_echo_n "checking for NE2000 support... " >&6; }
# Check whether --enable-ne2000 was given.
if test "${enable_ne2000+set}" = set; then :
  enableval=$enable_ne2000; if test "$enableval" = yes; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
    $as_echo "#define BX_NE2K_SUPPORT 1" >>confdefs.h

    NE2K_OBJS='ne2k.o eth.o eth_null.o'
    ac_fn_c_check_header_mongrel "$LINENO" "net/bpf.h" "ac_cv_header_net_bpf_h" "$ac_includes_default"
if test "x$ac_cv_header_net_bpf_h" = x""yes; then :
  NE2K_OBJS="$NE2K_OBJS eth_fbsd.o"
fi


   else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    $as_echo "#define BX_NE2K_SUPPORT 0" >>confdefs.h

    NE2K_OBJS=''
   fi
else

    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    $as_echo "#define BX_NE2K_SUPPORT 0" >>confdefs.h

    NE2K_OBJS=''


fi




{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for i440FX PCI support" >&5
$as_echo_n "checking for i440FX PCI support... " >&6; }
# Check whether --enable-pci was given.
if test "${enable_pci+set}" = set; then :
  enableval=$enable_pci; if test "$enableval" = yes; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
    $as_echo "#define BX_PCI_SUPPORT 1" >>confdefs.h

    PCI_OBJ='pci.o'
   else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    $as_echo "#define BX_PCI_SUPPORT 0" >>confdefs.h

    PCI_OBJ=''
   fi
else


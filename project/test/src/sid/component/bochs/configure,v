    $as_echo "#define BX_NUM_SIMULATORS 1" >>confdefs.h

   elif test "$enableval" = no; then
    $as_echo "#define BX_NUM_SIMULATORS 1" >>confdefs.h

   else
    cat >>confdefs.h <<_ACEOF
#define BX_NUM_SIMULATORS $enableval
_ACEOF

   fi
else

    $as_echo "#define BX_NUM_SIMULATORS 1" >>confdefs.h




{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for use of .cpp as suffix" >&5
$as_echo_n "checking for use of .cpp as suffix... " >&6; }
# Check whether --enable-cpp was given.
if test "${enable_cpp+set}" = set; then :
  enableval=$enable_cpp; if test "$enableval" = yes; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
    SUFFIX_LINE='.SUFFIXES: .cpp'
    CPP_SUFFIX='cpp'
   else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    SUFFIX_LINE='.SUFFIXES: .cc'
    CPP_SUFFIX='cc'
   fi
else

    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    SUFFIX_LINE='.SUFFIXES: .cc'
    CPP_SUFFIX='cc'


fi




if test "$enable_cpp" = yes; then
  echo "moving .cc source files to .cpp"
  sourcefiles=`find . -name "*.cc" -print`
  if test "$sourcefiles" != ""; then
    for ccname in $sourcefiles
    do
      cppname=`echo $ccname | sed -e "s/\.cc$/.cpp/"`
      echo "mv $ccname $cppname"
      mv   $ccname $cppname
    done
  else
    echo "no more .cc source files to rename"
  fi
fi


{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for Bochs internal debugger support" >&5
$as_echo_n "checking for Bochs internal debugger support... " >&6; }
# Check whether --enable-debugger was given.
if test "${enable_debugger+set}" = set; then :
  enableval=$enable_debugger; if test "$enableval" = yes; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
    $as_echo "#define BX_DEBUGGER 1" >>confdefs.h

    DEBUGGER_VAR='$(DEBUGGER_LIB)'
    bx_debugger=1
   else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    $as_echo "#define BX_DEBUGGER 0" >>confdefs.h

    DEBUGGER_VAR=''
    bx_debugger=0
   fi

else

    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    $as_echo "#define BX_DEBUGGER 0" >>confdefs.h

    DEBUGGER_VAR=''
    bx_debugger=0


fi



{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for disassembler support" >&5
$as_echo_n "checking for disassembler support... " >&6; }
# Check whether --enable-disasm was given.
if test "${enable_disasm+set}" = set; then :
  enableval=$enable_disasm; if test "$enableval" = yes; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
    $as_echo "#define BX_DISASM 1" >>confdefs.h

    DISASM_VAR='$(DISASM_LIB)'
   else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    if test "$bx_debugger" = 1; then
      echo "ERROR: debugger is enabled, so --enable-disasm is required"
      exit 1
    fi
    $as_echo "#define BX_DISASM 0" >>confdefs.h

    DISASM_VAR=''
   fi
else

    if test "$bx_debugger" = 1; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
      $as_echo "#define BX_DISASM 1" >>confdefs.h

      DISASM_VAR='$(DISASM_LIB)'
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
      $as_echo "#define BX_DISASM 0" >>confdefs.h

      DISASM_VAR=''
    fi


fi



{ $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to use readline" >&5
$as_echo_n "checking whether to use readline... " >&6; }
# Check whether --enable-readline was given.
if test "${enable_readline+set}" = set; then :
  enableval=$enable_readline; if test "$enableval" = yes; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
     want_readline=yes
   else
     { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
     want_readline=no
   fi
else
   # default is yes
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
    want_readline=yes

fi



if test "$want_readline" = yes; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for readline in -lreadline" >&5
$as_echo_n "checking for readline in -lreadline... " >&6; }
if test "${ac_cv_lib_readline_readline+set}" = set; then :
  $as_echo_n "(cached) " >&6
else
  ac_check_lib_save_LIBS=$LIBS
LIBS="-lreadline  $LIBS"
cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

/* Override any GCC internal prototype to avoid an error.
   Use char because int might match the return type of a GCC
   builtin and then its argument prototype would still apply.  */
#ifdef __cplusplus
extern "C"
#endif
char readline ();
int
main ()
{
return readline ();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"; then :
  ac_cv_lib_readline_readline=yes
else
  ac_cv_lib_readline_readline=no
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
LIBS=$ac_check_lib_save_LIBS
fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_cv_lib_readline_readline" >&5
$as_echo "$ac_cv_lib_readline_readline" >&6; }
if test "x$ac_cv_lib_readline_readline" = x""yes; then :

      if test "$bx_debugger" = 1; then
	# only add readline library if debugger is on
	$as_echo "#define HAVE_LIBREADLINE 1" >>confdefs.h

	READLINE_LIB='-lreadline'
      fi


fi

fi


ac_fn_c_check_header_mongrel "$LINENO" "readline/history.h" "ac_cv_header_readline_history_h" "$ac_includes_default"
if test "x$ac_cv_header_readline_history_h" = x""yes; then :
  $as_echo "#define HAVE_READLINE_HISTORY_H 1" >>confdefs.h


fi




{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for loader support" >&5
$as_echo_n "checking for loader support... " >&6; }
# Check whether --enable-loader was given.
if test "${enable_loader+set}" = set; then :
  enableval=$enable_loader; if test "$enableval" = yes; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
    $as_echo "#define BX_USE_LOADER 1" >>confdefs.h

    BX_LOADER_OBJS='bx_loader.o loader.o'
   else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    $as_echo "#define BX_USE_LOADER 0" >>confdefs.h

    BX_LOADER_OBJS=''
   fi
else

    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    $as_echo "#define BX_USE_LOADER 0" >>confdefs.h

    BX_LOADER_OBJS=''


fi





INSTRUMENT_DIR='instrument/stubs'

{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for instrumentation support" >&5
$as_echo_n "checking for instrumentation support... " >&6; }
# Check whether --enable-instrumentation was given.
if test "${enable_instrumentation+set}" = set; then :
  enableval=$enable_instrumentation; if test "$enableval" = yes; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
    $as_echo "#define BX_INSTRUMENTATION 1" >>confdefs.h

    INSTRUMENT_VAR='$(INSTRUMENT_LIB)'
   elif test "$enableval" = no; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    $as_echo "#define BX_INSTRUMENTATION 0" >>confdefs.h

    INSTRUMENT_VAR=''
   else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
    $as_echo "#define BX_INSTRUMENTATION 1" >>confdefs.h

    INSTRUMENT_DIR=$enableval
    INSTRUMENT_VAR='$(INSTRUMENT_LIB)'
   fi
else

    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    $as_echo "#define BX_INSTRUMENTATION 0" >>confdefs.h

    INSTRUMENT_VAR=''


fi




# Check whether --enable-simid was given.
if test "${enable_simid+set}" = set; then :
  enableval=$enable_simid; if test "$enableval" = yes; then
    $as_echo "#define BX_SIM_ID 0" >>confdefs.h

   elif test "$enableval" = no; then
    $as_echo "#define BX_SIM_ID 0" >>confdefs.h

   else
    cat >>confdefs.h <<_ACEOF
#define BX_SIM_ID $enableval
_ACEOF

   fi
else

    $as_echo "#define BX_SIM_ID 0" >>confdefs.h



fi



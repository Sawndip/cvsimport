  enableval=$enable_fpu; if test "$enableval" = yes; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
    $as_echo "#define BX_SUPPORT_FPU 1" >>confdefs.h


$as_echo "no" >&6; }
    $as_echo "#define BX_SUPPORT_FPU 0" >>confdefs.h

   else
    echo " "
    echo "ERROR: --enable-fpu does not accept a path"
    exit 1
   fi

else

    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
    $as_echo "#define BX_SUPPORT_FPU 1" >>confdefs.h


{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for port e9 hack" >&5
$as_echo_n "checking for port e9 hack... " >&6; }
# Check whether --enable-port-e9-hack was given.
if test "${enable_port_e9_hack+set}" = set; then :
  enableval=$enable_port_e9_hack; if test "$enableval" = yes; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
    $as_echo "#define BX_PORT_E9_HACK 1" >>confdefs.h

   else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    $as_echo "#define BX_PORT_E9_HACK 0" >>confdefs.h

   fi
else

    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    $as_echo "#define BX_PORT_E9_HACK 0" >>confdefs.h




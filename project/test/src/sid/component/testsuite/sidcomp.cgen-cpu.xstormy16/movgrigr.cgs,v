@d14 2
a15 2
	mov r1, #data
	mov.w r2, (r1)
d18 3
a20 3
	mov r2, #0xfc
	mov.b (r1), r2		; fc ce de ad
	mov.w r2, (r1)
d23 5
a27 5
	mov r2, #0xae
	add r1, #1
	mov.b (r1), r2		; fc ae de ad
	mov.w r2, (r1)
	assert r2, 0xfcae
d29 4
a32 4
	mov.w (r1), r2		; fc ae de ad
	sub r1, #1
	mov.w r2, (r1)
	assert r2, 0xfcae
@


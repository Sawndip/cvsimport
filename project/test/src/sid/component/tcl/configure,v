    ac_cv_prog_XSLTPROC="true"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

  test -z "$ac_cv_prog_XSLTPROC" && ac_cv_prog_XSLTPROC="false"
fi
fi

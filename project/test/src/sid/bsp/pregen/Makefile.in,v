install-data-local:
	$(mkinstalldirs) $(DESTDIR)$(pkgdatadir)
	@@for f in `cd $(srcdir); echo *.conf *.img` ; do \
		echo " $(INSTALL_DATA) $(srcdir)/$$f $(DESTDIR)$(pkgdatadir)/$$f"; \
		$(INSTALL_DATA) $(srcdir)/$$f $(DESTDIR)$(pkgdatadir)/$$f; \
	done


	$as_echo "#define PTRACE_ARG2_TYPE_LONG 1" >>confdefs.h

	$as_echo "#define PTRACE_ARG3_TYPE_LONG 1" >>confdefs.h

	$as_echo "#define PTRACE_ARG4_TYPE_LONG 1" >>confdefs.h

        ;;
    esac
    ;;
  mips*linux*)


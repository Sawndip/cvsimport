	$as_echo "#define PTRACE_ARG4_TYPE_LONG 1" >>confdefs.h

        ;;
      n32)

$as_echo "#define MIPS_ABI_N32 1" >>confdefs.h

	$as_echo "#define PTRACE_XFER_SIZE 8" >>confdefs.h

	$as_echo "#define PTRACE_XFER_TYPE_LONG_LONG 1" >>confdefs.h

	$as_echo "#define PTRACE_ARG1_TYPE_LONG_LONG 1" >>confdefs.h

	$as_echo "#define PTRACE_ARG2_TYPE_LONG_LONG 1" >>confdefs.h

	$as_echo "#define PTRACE_ARG3_TYPE_LONG_LONG 1" >>confdefs.h

	$as_echo "#define PTRACE_ARG4_TYPE_LONG_LONG 1" >>confdefs.h

        ;;
      n64)

$as_echo "#define MIPS_ABI_N64 1" >>confdefs.h

	$as_echo "#define PTRACE_XFER_SIZE 8" >>confdefs.h


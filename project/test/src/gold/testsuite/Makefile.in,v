	libgoldtest.a ../libgold.a ../../libiberty/libiberty.a $(LIBINTL_DEP)

LDADD = libgoldtest.a ../libgold.a ../../libiberty/libiberty.a $(LIBINTL) \
	$(THREADSLIB) $(LIBDL)

@@NATIVE_OR_CROSS_LINKER_TRUE@@object_unittest_SOURCES = object_unittest.cc

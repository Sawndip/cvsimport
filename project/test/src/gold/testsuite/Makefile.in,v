@@GCC_TRUE@@@@NATIVE_LINKER_TRUE@@	  echo 1>&2 "Link of debug_msg should have failed"; \
@@GCC_TRUE@@@@NATIVE_LINKER_TRUE@@	  rm -f $@@; \

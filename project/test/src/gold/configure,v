{ $as_echo "$as_me:${as_lineno-$LINENO}: checking whether $CC supports -fmerge-constants" >&5
$as_echo_n "checking whether $CC supports -fmerge-constants... " >&6; }
if test "${gold_cv_merge_constants+set}" = set; then :
  $as_echo_n "(cached) " >&6
else

save_CFLAGS="$CFLAGS"

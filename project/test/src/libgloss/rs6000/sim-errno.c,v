#include <errno.h>
#include <reent.h>

/* syscall handler branches here in case of error. */

int
_cerror (e)
     int e;
{
  _REENT->_errno = e;
  return -1;
}
@


1.1
log
@20000317 sourceware import
@
text
@d17 2
a18 7
int errno;

int *
__errno ()
{
  return &errno;
}
d26 1
a26 1
  errno = e;

#include <sched.h>
#include "jsre.h"


typedef struct {
  unsigned int res;
  unsigned int pad0[3];
} struct_sched_yield_t;

int
sched_yield (void)
{
  struct_sched_yield_t sys;

  return __send_to_ppe (JSRE_POSIX1_SIGNALCODE, JSRE_SCHED_YIELD, &sys);
}
@

#include <errno.h>
#include <sys/times.h>
#include "libnosys/warning.h"

/*
 * Only a stub version of times for now.
 */
clock_t
times (struct tms *buffer)
{
  errno = ENOSYS;
  return -1;
}

stub_warning (times)

AC_INIT(read.scm)
AC_CANONICAL_SYSTEM
dnl The "50" is a generic indicator that we're between 1.1 and 1.2.
AM_INIT_AUTOMAKE(cgen, 1.1.50)

AC_PROG_INSTALL
AC_EXEEXT

# Set target cpu.
arch=${target_cpu}
AC_SUBST(arch)

AM_MAINTAINER_MODE


  picochip-*-*)
    noconfigdirs="$noconfigdirs target-libiberty"
    ;;
  mips*-sde-elf*)
    skipdirs="$skipdirs target-libiberty"
    ;;
  ip2k-*-*)
    noconfigdirs="$noconfigdirs target-libiberty"
    ;;
esac


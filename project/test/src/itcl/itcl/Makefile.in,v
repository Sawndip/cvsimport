a51 25
WIN_OBJECTS = dllEntryPoint.$(OBJEXT)
UNIX_OBJECTS =
GENERIC_OBJECTS = itclStubInit.$(OBJEXT) \
	itcl_bicmds.$(OBJEXT) \
	itcl_class.$(OBJEXT) \
	itcl_cmds.$(OBJEXT) \
	itcl_ensemble.$(OBJEXT) \

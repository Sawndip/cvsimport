tclIndex: $(TCL)
	TCL_LIBRARY=$(srcdir)/../../tcl/library; export TCL_LIBRARY; \
	here=`pwd`; \
	$(SET_LIB_PATH) \
	cd $(srcdir) && \
	  echo "auto_mkindex $(LIBGUI_LIBRARY_DIR) $(TCL)" | @@ITCL_SH@@

pkgIndex.tcl: @@MAINT@@ $(PACKAGES)
	here=`pwd`;	 \
	$(SET_LIB_PATH) \
	cd $(srcdir) && \
	echo "pkg_mkIndex . $(PACKAGES); exit" | @@ITCL_SH@@

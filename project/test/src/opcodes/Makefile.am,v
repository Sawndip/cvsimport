	$(CFLAGS_FOR_BUILD)
LINK_FOR_BUILD = $(CC_FOR_BUILD) $(CFLAGS_FOR_BUILD) \
	$(LDFLAGS_FOR_BUILD) -o $@@

libopcodes_la_LDFLAGS =
if INSTALL_LIBBFD
bfdlibdir = @@bfdlibdir@@

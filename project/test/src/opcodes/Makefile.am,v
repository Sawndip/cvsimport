noinst_LTLIBRARIES = libopcodes.la
libopcodes_la_LDFLAGS += -rpath $(rpath_bfdlibdir)
endif

# This is where bfd.h lives.
BFD_H = ../bfd/bfd.h

BUILD_LIBS = @@BUILD_LIBS@@
BUILD_LIB_DEPS = @@BUILD_LIB_DEPS@@


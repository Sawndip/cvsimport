#define OpMXX6aHintHlf(a,b,c,d,e,f) \
        (bOp (a) | bM (b) | bX (c) |           bX6a (d) | bHint (e) | bHlf(f)), \
	(mOp | mM | mX |       mX6a | mHint | mHlf)

#define bX7(x)		(((ia64_insn) ((x) & 0x1)) << 36)	/* note: alias for bM() */
#define bY(x)		(((ia64_insn) ((x) & 0x1)) << 26)
a44 2
#define mX7	bX7 (-1)
#define mY	bY (-1)
a49 2
#define OpX3X6bX7(a,b,c,d)	(bOp (a) | bX3 (b) | bX6b (c) | bX7 (d)), \
				(mOp | mX3 | mX6b | mX7)
a53 2
#define OpX3X4X2Y(a,b,c,d,e) 	(bOp (a) | bX3 (b) | bX4 (c) | bX2 (d) | bY (e)), \
				(mOp | mX3 | mX4 | mX2 | mY)

static int
print_insn_arg (d, l, pc, info)
d674 1
a674 1
  int op, delta, consumed;
d676 1
a676 2
  consumed = 1;
  switch (*d)
a677 12
    case ',':
    case '(':
    case ')':
    case '[':
    case ']':
      (*info->fprintf_func) (info->stream, "%c", *d);
      break;

    case '+':
      /* Extension character; switch for second char.  */
      d++;
      consumed++;

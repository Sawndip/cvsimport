  (((insn) >> OP_SH_##field) & OP_MASK_##field)
#define GET_OP_S(insn, field) \
  ((GET_OP (insn, field) ^ ((OP_MASK_##field >> 1) + 1)) \
   - ((OP_MASK_##field >> 1) + 1))

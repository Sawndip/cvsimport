#line 1035 "configure"
#include "confdefs.h"

#define WIN32_LEAN_AND_MEAN
#include <windows.h>
#undef WIN32_LEAN_AND_MEAN

int main() {

  EXCEPTION_DISPOSITION x;

; return 0; }
EOF

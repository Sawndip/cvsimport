        TCL_LIB_FULL_PATH="`echo $val | sed -e s#\\\\\\\\#/#g`"
      fi
    ;;
    *)
      # Default to a no-op under Unix or Cygwin gcc
      TCL_LIB_FULL_PATH=$val
    ;;
  esac






1.1.1.2
log
@Tcl 8.3 upgrade
@
text
@d1 1
a1 1
// RCS: @@(#) $Id: tcl.rc,v 1.6.8.2 2000/09/15 16:58:42 spolk Exp $
a5 2
#define VS_VERSION_INFO 1

a8 2
LANGUAGE 0x9, 0x1	/* LANG_ENGLISH, SUBLANG_DEFAULT */

d14 2
a15 2
 FILEOS 	0x4	/* VOS__WINDOWS32 */
 FILETYPE 	0x2	/* VFT_DLL */
d20 1
a20 1
        BLOCK "040904b0" /* LANG_ENGLISH/SUBLANG_ENGLISH_US, Unicode CP */
d24 1
a24 1
            VALUE "CompanyName", "Scriptics Corporation\0"
d26 1
a26 1
            VALUE "LegalCopyright", "Copyright (c) 2000 by Scriptics Corporation\0"
d29 1
a29 1
        END		    
a35 3



@


1.1.1.3
log
@import tcl 8.4.0
@
text
@d1 1
a1 1
// RCS: @@(#) $Id: tcl.rc,v 1.8 2002/06/18 00:12:24 davygrvy Exp $
d3 1
a3 1
// Version Resource Script
d6 3
a8 1
#include <winver.h>
a10 18
//
// build-up the name suffix that defines the type of build this is.
//
#ifdef TCL_THREADS
#define SUFFIX_THREADS	    "t"
#else
#define SUFFIX_THREADS	    ""
#endif

#ifdef DEBUG
#define SUFFIX_DEBUG	    "d"
#else
#define SUFFIX_DEBUG	    ""
#endif

#define SUFFIX		    SUFFIX_THREADS SUFFIX_DEBUG


a16 3
#ifdef DEBUG
 FILEFLAGS 	VS_FF_DEBUG
#else
d18 2
a19 3
#endif
 FILEOS 	VOS__WINDOWS32
 FILETYPE 	VFT_DLL
d27 2
a28 2
            VALUE "OriginalFilename", "tcl" STRINGIFY(JOIN(TCL_MAJOR_VERSION,TCL_MINOR_VERSION)) SUFFIX ".dll\0"
            VALUE "CompanyName", "ActiveState Corporation\0"
d30 1
a30 1
            VALUE "LegalCopyright", "Copyright \251 2001 by ActiveState Corporation, et al\0"
d40 10
@


